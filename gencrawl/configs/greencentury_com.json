{
    "website": "https://www.greencentury.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "greencentury.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_greencentury_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.greencentury.com/balanced-fund/",
            "https://www.greencentury.com/international-index-fund/",
            "https://www.greencentury.com/equity-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//span[contains(text(),'Fund')]/text()[not(contains(.,'Daily'))][not(contains(.,'Invest'))]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//td[contains(text(),'Share Class')]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split('-')[-1].split('(')[-1].split(')')[0]"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('-')[-1].split('(')[0]"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_consultant": {
                "paths": [
                    "//span[contains(text(),'Investment Advisor')]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_consultant'].split(':')[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//p[contains(text(),'As of')]/text()[contains(.,'Total Net Assets')]"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets'].split(':')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//p[contains(text(),'As of')]/text()[contains(.,'As of')]"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('of')[-1].replace(':','')"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}