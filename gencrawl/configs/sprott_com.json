{
    "website": "https://www.sprott.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "sprott.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.sprott.com/investment-strategies/sprott-gold-equity-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[@class = 'etfs-h1']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//h5[contains(text(),'Fund Details')]/following::strong[position()=1 or position()=2]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split(' In')[0]"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/following-sibling::td/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()[contains(.,'Class')]"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "total_net_assets": {
                "paths": [
                    "//td[contains(text(),'Total Fund Assets')]/following-sibling::td/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//h5[contains(text(),'Fund Details')]/span/text()"
                ],
                "cleanup_functions": [
                    "obj['total_net_assets_date'].split('of ')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//td[contains(text(),'Distribution and Service (12b-1) Fee')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "other_expenses": {
                "paths": [
                    "//td[contains(text(),'Other Expenses')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//td[contains(text(),'Management Fee')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//td[contains(text(),'Redemption Fee')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['redemption_fee'].split('%')[0] + '%'"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//td[contains(text(),'Sales Charge')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Gross Annual Fund Operating Expenses')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h5[contains(text(),'Fund Objective')]/following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Investment')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "[m.split('(')[0] for m in obj['minimum_initial_investment']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//td[contains(text(),'Turnover')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h2[contains(text(),'Portfolio')]/span/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].split('of ')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//td[contains(text(),'Dividend Policy ')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//td[contains(text(),'Index')]/text()"
                ],
                "cleanup_functions": [
                    "list(set(obj['benchmarks']))"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class = 'person-card-data']"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./span[1]/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}