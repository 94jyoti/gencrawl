{
    "website": "https://ancora.net",
    "spider": "financial_detail",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "ancora.net"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://ancora.net/products/ancora-dividend-value-equity-fund/",
            "https://ancora.net/products/ancora-microcap-fund/",
            "https://ancora.net/products/ancora-income-fund/"
        ],
        "ext_codes": {
            "share_class": {
                "paths": [
                    "//td[contains(text(),'Share Class')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].replace('Share', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@id ='people-region']/div/h3"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./a/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "benchmarks": {
                "paths": [
                    "//td[contains(text(),'Index') or contains(text(),'Benchmark')]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Gross Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Net Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//div[@id ='objective-region']/div/p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//td[contains(text(),'Max. Sales Charge')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}