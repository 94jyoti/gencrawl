{
    "website": "https://www.guggenheiminvestments.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "guggenheiminvestments.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "wait_time": 5,
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvdax-rbp-large-cap-defensive",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvdcx-rbp-large-cap-defensive",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tveax-rbp-dividend",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvecx-rbp-dividend",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvefx-rbp-dividend",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tveix-rbp-dividend",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvfdx-rbp-large-cap-defensive",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvfmx-rbp-large-cap-market",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvfrx-directional-allocation",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvidx-rbp-large-cap-defensive",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvimx-rbp-large-cap-market",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvmax-rbp-large-cap-market",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvmcx-rbp-large-cap-market",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvrax-directional-allocation",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvrcx-directional-allocation",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvrix-directional-allocation",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvvax-rbp-large-cap-value",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvvcx-rbp-large-cap-value",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvvfx-rbp-large-cap-value",
            "https://www.guggenheiminvestments.com/mutual-funds/fund/tvvix-rbp-large-cap-value"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "(//h1/text())[2]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "(//h1/text())[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//div[@class='btn-group fund-class-selector']/button/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_consultant": {
                "paths": [
                    "//td[contains(text(),' Investment Adviser')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Gross Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Net Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h3[contains(text(),'Investment Strategy')]/parent::div/div[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//td[contains(text(),' Distribution Frequency ')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['distribution_frequency'].split('(')[0] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//td[contains(text(),'Subsidized SEC Yield ')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "(//label[@class='small']/text())[2]"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_date_30_day'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_30_day": {
                "paths": [
                    "//td[contains(text(),'Unsubsidized SEC Yield ')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_date_30_day": {
                "paths": [
                    "(//label[@class='small']/text())[2]"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_without_waivers_date_30_day'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "distribution_yield_12_month": {
                "paths": [
                    "//td[contains(text(),' Distribution Yield')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//td[contains(text(),' Benchmark')]/following-sibling::td/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "(//h3[contains(text(),'Investment Team')]/following-sibling::div/table/tbody/tr[1])[1]/td"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}