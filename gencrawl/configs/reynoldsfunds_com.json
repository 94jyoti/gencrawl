{
    "website": "https://www.reynoldsfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "reynoldsfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.reynoldsfunds.com/fund-facts.html"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class='footertext']"
                ],
                "cleanup_functions": [
                    "obj['instrument_name'].split('2021')[-1].split('|')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//b[contains(text(),'Symbol:')]/parent::td/following-sibling::td[1]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "//b[contains(text(),'Category:')]/parent::td/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//b[contains(text(),'Net Assets in Millions:')]/parent::td/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "(//td[contains(text(),'(as of')])[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace('(as of', '').replace(')', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "initial_sales_charge": {
                "paths": [
                    "//b[contains(text(),'Front End Load')]/text()"
                ],
                "cleanup_functions": [
                    "obj['initial_sales_charge'].split(':')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//b[contains(text(),'Redemption Fee')]/text()"
                ],
                "cleanup_functions": [
                    "obj['redemption_fee'].split(':')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//b[contains(text(),'Net Expense Ratio')]/parent::span/following-sibling::font[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//b[contains(text(),'Inception Date:')]/parent::td/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h2[contains(text(),'Investment Objective')]/following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h1[contains(text(),'Fund Facts')]/following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Initial:')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//td[contains(text(),'Subsequent:')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//b[contains(text(),'Turnover Rate')]/parent::span/following-sibling::font/span[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//b[contains(text(),'Turnover Rate')]/parent::span/following-sibling::font/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].replace('(as of', '').replace(')', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h2[contains(text(),'Portfolio Manager')]/following-sibling::p[1]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}