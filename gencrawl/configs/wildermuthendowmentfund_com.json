{
    "website": "https://wildermuthendowmentfund.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "wildermuthendowmentfund.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://wildermuthendowmentfund.com/fund-overview/"
        ],
        "ext_codes": {
            "temp_class_blocks": {
                "paths": [
                    "//th[contains(text(),'Fund Information')]/following-sibling::th"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "instrument_name":{
             "paths":[
                 "//title//text()"
             ],
             "cleanup_functions":["obj['instrument_name'].split('|')[-1].strip()"],
             "return_type": "str",
             "selector": "root"
            
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_consultant": {
                "paths": [
                    "//h3[contains(text(),'Founder and Portfolio Manager')]/parent::div/h4/following-sibling::div/following-sibling::p/a//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h3[contains(text(),'Investment Objective')]/following-sibling::p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h3[contains(text(),'Investment Strategy')]/following-sibling::p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Initial')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "[o.split('/')[0] for o in obj['minimum_initial_investment']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Initial')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "[o.split('/')[-1] for o in obj['minimum_additional_investment']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//td[contains(text(),'Dividend Frequency')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            }
        }
    }
}