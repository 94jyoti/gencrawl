{
    "website": "http://usfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "usfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_usfunds_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.usfunds.com/our-funds/our-mutual-funds/global-resources-fund/overview/",
            "https://www.usfunds.com/our-funds/our-mutual-funds/gold-and-precious-metals-fund/overview/",
            "https://www.usfunds.com/our-funds/our-mutual-funds/world-precious-minerals-fund/overview/",
            "https://www.usfunds.com/our-funds/our-mutual-funds/emerging-europe-fund/overview/",
            "https://www.usfunds.com/our-funds/our-mutual-funds/china-region-fund/overview/",
            "https://www.usfunds.com/our-funds/our-mutual-funds/global-luxury-goods-fund/overview/",
            "https://www.usfunds.com/our-funds/our-mutual-funds/near-term-tax-free-fund/overview/",
            "https://www.usfunds.com/our-funds/our-mutual-funds/us-government-securities-ultra-short-bond-fund/overview/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/text()"
                ],
                "cleanup_functions": [
                    "obj['instrument_name'].split('(')[0] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//strong[contains(text(),'Ticker:')]/parent::p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets": {
                "paths": [
                    "//acronym[@title='Assets Under Management']/parent::strong/parent::p/text()"
                ],
                "cleanup_functions": [
                    "obj['total_net_assets'].split('as of')[0] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//acronym[@title='Assets Under Management']/parent::strong/parent::p/text()"
                ],
                "cleanup_functions": [
                    "obj['total_net_assets_date'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//strong[contains(text(),'Inception Date')]/parent::p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h2[contains(text(),'Fund Objective')]/following-sibling::p[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h2[contains(text(),'Fund Strategy')]/following-sibling::p[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//h2[contains(text(), 'Fund Strategy')]/following::p[contains(text(),'benchmark is the')]/text()"
                ],
                "cleanup_functions": [
                    "[o.split('benchmark is the')[-1] for o in obj['benchmarks']]"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}