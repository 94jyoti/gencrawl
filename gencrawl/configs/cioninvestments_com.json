{
    "website": "https://www.cioninvestments.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "cioninvestments.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.cioninvestments.com/products/cion-ares-diversified-credit-fund/#A"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[contains(text(),'Fund')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//strong[contains(text(),'CLASS')]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//strong[contains(text(),'CLASS')]/parent::h2/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//span[contains(text(),'TOTAL MANAGED ASSETS')]/ancestor::tr/td[2]/text()"
                ],
                "cleanup_functions": [
                    "[i.split('~')[-1] for i in obj['portfolio_assets']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h3[contains(text(),'Fund Facts')]/following-sibling::h4/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('of')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//span[contains(text(),'INCEPTION')]/ancestor::td/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//span[contains(text(),'DISTRIBUTIONS')]/ancestor::tr/td[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class = 'each-bio']/div"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./following-sibling::h5"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    "./following-sibling::p[2]"
                ],
                "cleanup_functions": [
                    "obj['fund_manager_years_of_experience_in_industry'].split(':')[-1]"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}