{
    "website": "https://www.nb.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "nb.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.nb.com/en/us/products/mutual-funds/absolute-return-multi-manager-fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class=('fund-info col-xs-12 col-sm-6')]/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//div[contains(text(),'Symbol')]/following-sibling::div"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//div[contains(text(),'CUSIP')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "(//div[contains(text(),'Share Class')]/following-sibling::div)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//div[contains(text(),'Total Portfolio Assets')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//div[contains(text(),'Total Portfolio Assets')]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace('Total Portfolio Assets As of ', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//div[contains(text(),'Gross Expense Ratio')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//div[contains(text(),'Net Expense Ratio')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//div[contains(text(),'Inception Date')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class='row bio-group']/div"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//div//a/h6/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    ".//div/following-sibling::div/div[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['fund_manager_years_of_experience_in_industry'].replace(' of Industry Experience', '')"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        },
        "pagination": {
            "pagination": {
                "paths": [
                    "//ul[@class='dropdown-list']//li/a/@href"
                ],
                "cleanup_functions": [
                    "[obj['fund_url']+p for p in obj['pagination']]"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}