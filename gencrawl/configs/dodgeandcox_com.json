{
    "website": "https://dodgeandcox.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "dodgeandcox.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_dodgeandcox_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://dodgeandcox.com/incomefund.asp",
            "https://dodgeandcox.com/globalfund.asp",
            "https://dodgeandcox.com/ISF.asp",
            "https://dodgeandcox.com/EMSF.asp",
            "https://dodgeandcox.com/balancedfund.asp",
            "https://dodgeandcox.com/stockfund.asp",
            "https://dodgeandcox.com/GBF.asp"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class='section-title']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//th[contains(text(),'Ticker')]/parent::tr/td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//th[contains(text(),'CUSIP')]/parent::tr/td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//span[contains(text(),'Fund Characteristics')]/following-sibling::span/text()"
                ],
                "cleanup_functions": [
                    "obj['total_net_assets_date'].split('unless')[0]",
                    "obj['total_net_assets_date'].replace('as of',' ')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//span[contains(text(),'Inception Date')]/parent::th/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//span[contains(normalize-space(text()),'objective')]/parent::td/span[2]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//span[contains(text(),'Strategy')]/following-sibling::*//text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//th[contains(text(),'Minimum Initial Investment')]/parent::tr/td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//th[contains(text(),'Subsequent Investment')]/parent::tr/td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//th[contains(text(),'Distributions')]/parent::tr/td/text()"
                ],
                "cleanup_functions": [
                    "obj['dividend_frequency'].split(';')[0]",
                    "obj['dividend_frequency'].split('in')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//td[contains(text(),'Investment Committee')]/parent::tr/following-sibling::tr[3]/td/div/p"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./span/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}