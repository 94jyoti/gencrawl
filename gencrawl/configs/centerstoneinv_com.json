{
    "website": "https://centerstoneinv.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "centerstoneinv.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://centerstoneinv.com/strategies/centerstone-investors-fund/",
            "https://centerstoneinv.com/strategies/centerstone-international-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@id = 'page-title']/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//div[@class = 'ticker']"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./div/span/following-sibling::h2"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "./div/span/text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//span[contains(text(),'Total Net')]/following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h3[contains(text(),'Portfolio Characteristics')]/following-sibling::div/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split(':')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "annual_fund_operating_expenses_after_fee_waiver": {
                "paths": [
                    "//th[contains(text(),'Net Total Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//th[contains(text(),'Distribution & Service')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//th[contains(text(),'Management Fees')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "contingent_deferred_sales_charge": {
                "paths": [
                    "//th[contains(text(),'CDSC')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//th[contains(text(),'Redemption Fees')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//th[contains(text(),'Maximum Sales')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Total Operating Expense')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//th[contains(text(),'Net Total Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//span[contains(text(),'Inception')]/following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "(//div[@class= 'objective']/text())[2]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//th[contains(text(),'Minimum')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//span[contains(text(),'Portfolio Turnover')]/following-sibling::span/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate'].split('*')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h3[contains(text(),'Portfolio Characteristics')]/following-sibling::div/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].split(':')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//span[contains(text(),'Dividend')]/following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//td[contains(text(),'Index')]/text()"
                ],
                "cleanup_functions": [
                    "obj['benchmarks'][:2]"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h3[contains(text(),'Portfolio Manager')]/following-sibling::div/h3/a"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}