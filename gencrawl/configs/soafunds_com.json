{
    "website": "https://www.soafunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "soafunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.soafunds.com/soaex",
            "https://www.soafunds.com/soamx",
            "https://www.soafunds.com/soaix",
            "https://www.soafunds.com/soaox",
            "https://www.soafunds.com/soavx",
            "https://www.soafunds.com/soaax"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "(//h2[@class=\"font_2\"])[1]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//*[contains(text(),'Fund Symbol')]//following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//*[contains(text(),'Net Assets')]//following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//*[contains(text(),'Gross Expense Ratio')]//following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//*[contains(text(),'Net Expense Ratio')]//following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//*[contains(text(),'Inception Date')]//following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//*[contains(text(),'Fund Objective')]/following::span[1]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//*[contains(text(),'Investment Strategy')]/following::ul[1]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//*[contains(text(),'Distribution Frequency')]//following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//*[contains(text(),\"Management Team\")]//following::span[contains(text(),\"Portfolio Manager\")]//preceding::span[1]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}