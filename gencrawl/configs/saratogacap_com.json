{
    "website": "https://www.saratogacap.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "saratogacap.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.saratogacap.com/conservative-balanced",
            "https://www.saratogacap.com/moderately-conservative-allocation",
            "https://www.saratogacap.com/moderate-allocation",
            "https://www.saratogacap.com/moderate-aggressive-allocation",
            "https://www.saratogacap.com/aggressive-allocation",
            "https://www.saratogacap.com/large-capitalization-value",
            "https://www.saratogacap.com/large-capitalization-growth",
            "https://www.saratogacap.com/mid-capitalization",
            "https://www.saratogacap.com/small-capitalization",
            "https://www.saratogacap.com/international-equity",
            "https://www.saratogacap.com/health-and-biotechnology",
            "https://www.saratogacap.com/technology-and-communications",
            "https://www.saratogacap.com/financial-services",
            "https://www.saratogacap.com/energy-and-basic-materials",
            "https://www.saratogacap.com/investment-quality-bond",
            "https://www.saratogacap.com/municipal-bond",
            "https://www.saratogacap.com/us-government-money-market"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "(//h1)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//h1[contains(text(),\"Fund\")]/following::*[contains(text(),\"shares\")]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//h1[contains(text(),\"Fund\")]/following::*[contains(text(),\"shares\")]//following-sibling::text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('Symbol')[0] "
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_inception_date": {
                "paths": [
                    "//h1[contains(text(),\"Fund\")]/following::*[contains(text(),\"Inception\")]//following-sibling::text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//h1[contains(text(),\"Fund\")]/following::*[contains(text(),\"Minimum Investment:\")]//following-sibling::text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}