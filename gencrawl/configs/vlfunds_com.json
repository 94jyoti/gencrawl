{
    "website": "https://vlfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "vlfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://vlfunds.com/funds/equity/midcapfocus/characteristics",
            "https://vlfunds.com/funds/equity/largercompanies/characteristics",
            "https://vlfunds.com/funds/equity/smallcap/characteristics",
            "https://vlfunds.com/funds/equity/selectgrowth/characteristics",
            "https://vlfunds.com/funds/hybrid/assetallocation/characteristics",
            "https://vlfunds.com/funds/hybrid/capitalappreciation/characteristics",
            "https://vlfunds.com/funds/fixed-income/corebond/characteristics",
            "https://vlfunds.com/funds/fixed-income/taxexempt/characteristics"
            
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//h1/parent::div/div/div/div/following-sibling::div/div[1]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split('(')[0] "
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "//h2[contains(text(),'Fees & Expenses')]/parent::div/ul/li/a/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//p[contains(text(),'Net Assets')]/parent::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//p[contains(text(),'Net Assets')]/parent::div/preceding-sibling::div//text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('As Of')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//td[contains(text(),'Redemption Fee')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Expense Ratio Before Waivers')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Expense Ratio After Waivers')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Initial Investment')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Subsequent Investment')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//table[@id='aartable']/tbody/tr[4]/td[2]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//span[contains(text(),'Portfolio Manager')]/parent::h3"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}