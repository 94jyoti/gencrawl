{
    "website": "https://www.aamlive.com/",
    "spider": "financial_detail",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "www.aamlive.com/"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.aamlive.com/publicsite/mutual-funds-detail-bahl-gaynor"
        ],
        "ext_codes": {
            "temp_class_rows": {
                "paths": [
                    "//div[contains(text(), 'Tickers')]/parent::div/following-sibling::table//tr"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./td[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].replace('Class', '')",
                    "obj['share_class'].replace(':', '')"
                ],
                "return_type": "str",
                "selector": "temp_class_rows"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./td[2]/text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split('/')[0]"
                ],
                "return_type": "str",
                "selector": "temp_class_rows"
            },
            "cusip": {
                "paths": [
                    "./td[2]/text()"
                ],
                "return_type": "str",
                "selector": "temp_class_rows",
                "cleanup_functions": [
                    "obj['cusip'].split('/')[-1]"]
            },
            "instrument_name": {
                "paths": [
                    "//div[contains(@class,'title banner')]//following-sibling::h1//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h4[contains(text(),' Portfolio characteristics ')]//small//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//div[contains(text(),'Portfolio turnover (last fiscal year)')]//following-sibling::div//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//p[contains(text(), 'Objective')]/following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//div[text()='Inception Date']/parent::div/following-sibling::table//td[last()]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
             "temp_expense_gross": {
                "paths": [
                    "//div[contains(text(), 'Expense Ratio')]/parent::div/following-sibling::table//tr//td[2][contains(text(),'gross')]//text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_expense_net":{
             "paths":["//div[contains(text(), 'Expense Ratio')]/parent::div/following-sibling::table//tr//td[2][contains(text(),'net')]//text()"],
             "return_type": "list",
             "selector": "root"
            }
            
        }
    }
}