{
    "website": "https://keeleyfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "keeleyfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "custom_settings":{
         "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://keeleyfunds.com/funds/ksdvx/profile.php",
            "https://keeleyfunds.com/funds/kmdvx/profile.php",
            "https://keeleyfunds.com/funds/ksmvx/profile.php"
        ],
        "ext_codes": {
        	"retry_condition": {
        	"paths": [
          		"//title[contains(text(), 'One moment, please')]"
        			]
      		},
        
        
            "instrument_name": {
                "paths": [
                    "//div[@id=\"title\"]/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//*[contains(text(),\"Shares:\")]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split(':')[-1] "
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('Shares')[0] "
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//*[contains(text(),\"Fund Profile \")]//following::td[contains(text(),\"Total Net Assets\")]//following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//*[contains(text(),\"Fund Profile \")]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('of ')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//*[contains(text(),\"Expense Ratios\")]//following::td[contains(text(),\"Gross Expense Ratio\")]//following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//*[contains(text(),\"Expense Ratios\")]//following::td[contains(text(),\"K\")]//following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//*[contains(text(),\"Investment Objective\")]//following-sibling::p[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}