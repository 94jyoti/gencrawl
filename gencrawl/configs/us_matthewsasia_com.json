        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "(//div[@class='container']//h2)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'Cusip')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//select[@id='share_class-select']/text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Fund Assets')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets'].split('(')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//td[contains(text(),'Fund Assets')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('(')[-1].replace(')', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "(//td[contains(text(),'Gross Expense Ratio')]/following-sibling::td)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "(//td[contains(text(),'Net Expense Ratio')]/following-sibling::td)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "(//h5[contains(text(),'Objective')]/following-sibling::p)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "(//h5[contains(text(),'Strategy')]/following-sibling::p)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//td[contains(text(),'Portfolio Turnover')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//a[contains(text(),'Benchmark')]/parent::td/following-sibling::td/text()",
                    "//a[contains(text(),'Benchmark')]/parent::td/following-sibling::td/span/text()"
                ],
                "cleanup_functions": [
                    "obj['benchmarks'][:1]"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class='team-members']//h5"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}