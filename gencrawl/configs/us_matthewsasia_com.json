{
    "website": "https://us.matthewsasia.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "us.matthewsasia.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://us.matthewsasia.com/funds/global-emerging-markets/emerging-markets-equity-fund/",
            "https://us.matthewsasia.com/funds/global-emerging-markets/emerging-markets-small-companies-fund/",
            "https://us.matthewsasia.com/funds/asia-growth/asia-growth/",
            "https://us.matthewsasia.com/funds/asia-growth/pacific-tiger-fund/",
            "https://us.matthewsasia.com/funds/asia-growth/asia-esg-fund/",
            "https://us.matthewsasia.com/funds/asia-growth/asia-innovators-fund/",
            "https://us.matthewsasia.com/funds/asia-growth/china-fund/",
            "https://us.matthewsasia.com/funds/asia-growth/china-small-companies-fund/",
            "https://us.matthewsasia.com/funds/asia-growth/india-fund/",
            "https://us.matthewsasia.com/funds/asia-growth/japan-fund/",
            "https://us.matthewsasia.com/funds/asia-growth/korea-fund/",
            "https://us.matthewsasia.com/funds/growth-and-income/growth-and-income-fund/",
            "https://us.matthewsasia.com/funds/growth-and-income/asia-dividend-fund/",
            "https://us.matthewsasia.com/funds/growth-and-income/china-dividend-fund/",
            "https://us.matthewsasia.com/funds/fixed-income/asia-total-return-bond-fund/",
            "https://us.matthewsasia.com/funds/fixed-income/asia-credit-opportunities-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "(//div[@class='container']//h2)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'Cusip')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Fund Assets')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets'].split('(')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//td[contains(text(),'Fund Assets')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('(')[-1].replace(')', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "(//td[contains(text(),'Gross Expense Ratio')]/following-sibling::td)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "(//td[contains(text(),'Net Expense Ratio')]/following-sibling::td)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "(//h5[contains(text(),'Objective')]/following-sibling::p)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "(//h5[contains(text(),'Strategy')]/following-sibling::p)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//td[contains(text(),'Portfolio Turnover')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//a[contains(text(),'Benchmark')]/parent::td/following-sibling::td//text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class='team-members']//h5"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        },
        "pagination": {
            "pagination": {
                "paths": [
                    "//div[@class='share-class']//option[position()>1]/@value"
                ],
                "cleanup_functions": [
                    "[(obj['fund_url'].split('?')[0] + '?FundClassType=' + o) for o in obj['pagination']]"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}