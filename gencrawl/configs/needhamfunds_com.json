{
    "website": "https://www.needhamfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "needhamfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.needhamfunds.com/mutual-funds/growth-fund/",
            "https://www.needhamfunds.com/mutual-funds/aggressive-growth-fund/",
            "https://www.needhamfunds.com/mutual-funds/small-cap-growth-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//h3[contains(text(),'Fund Statistics')]/parent::div/div/table/thead/tr/th/following-sibling::th"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker Symbols')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//div[contains(text(),'Net Assets')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h3[contains(text(),'Portfolio Characteristics')]/span/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace('as of',' ')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//td[contains(text(),'12B-1 Fees')]/following-sibling::td/text()",
                    "//td[contains(text(),'12b-1 Fees')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//td[contains(text(),'Redemption Fee')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Gross Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Net Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h3[contains(text(),'Investment Objective')]/parent::div/div/div/p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//div[contains(text(),'Strategy')]/parent::div/ul//text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Initial Investment')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "[o.split('for')[0] for o in obj['minimum_initial_investment']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Subsequent Investment')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "[o.split('for')[0] for o in obj['minimum_additional_investment']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//div[contains(text(),'Turnover')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h3[contains(text(),'Portfolio Characteristics')]/span/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].replace('as of',' ')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h3[contains(text(),'Portfolio Managers')]/parent::div/div/a/div/h4"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}