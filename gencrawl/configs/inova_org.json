{
    "website": "https://www.inova.org/",
    "domain": "hospital",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "inova.org"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "//h2[@id=\"provider-name\"]//text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "speciality": {
                "paths": [
                    "//div[contains(@id,'about-panel-specialties')]//div[contains(@class,'is-hierarchy')]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "affiliation": {
                "paths": [
                    "//div[contains(@id,'network_affiliations')]//div[contains(@class,'about-panel-item')]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "address_raw": {
                "paths": [
                    "//div[contains(@itemprop,'address')]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            }
        }
    }
}