{
  "website": "www.nuveen.com",
  "spider": "financial_detail",
  "domain": "financial",
  "crawl_method": "SCRAPY_GET",
  "allowed_domains": [
    "www.nuveen.com",
    "api.nuveen.com"
  ],
  "parsing_type": "xpath",
  "country": "US",
  "language": "ENGLISH",
  "listing": {
    "crawl_method": "SCRAPY_GET",
    "parsing_type": "xpath",
    "start_urls": [
      "https://www.nuveen.com/en-us/mutual-funds/nuveen-limited-term-municipal-bond-fund?shareclass=I"
    ]
  },
  "detail": {
    "crawl_method": "SELENIUM",
    "wait_time": 8,
	"parsing_type": "xpath",
    "custom_settings": {
      "HTTPCACHE_ENABLED": false
    },
    "ext_codes": {
      "retry_condition": {
        "paths": [
          "//title[contains(text(), 'Nuveen Site Unavailable')]"
        ]
      },
      "instrument_name": {
        "paths": [
          "//*[@class='nuv-product-detail-hero-retail__main-title no-outline']//text()"
        ]
      },
      "nasdaq_ticker": {
        "paths": [
          "//*[contains(text(), 'Symbol')]//following-sibling::div//span//text()"
        ]
      },
      "cusip": {
        "paths": [
          "//*[contains(text(), 'CUSIP')]//following-sibling::div//span//text()"
        ]
      },
      "portfolio_assets": {
        "paths": [
          "//h3[contains(text(), 'Fund Net Assets')]//following-sibling::div//span//text()"
        ]
      },
      "portfolio_assets_date": {
        "paths": [
          "//*[@class='nuv-data-callout__date']//following-sibling::span//text()",
          "//h3[contains(text(), 'Fund Net Assets')]//following-sibling::div//div[2]//span//text()"
        ]
      },
      "total_expense_gross": {
        "paths": [
          "//*[contains(text(), 'Gross Expense Ratio')]//following-sibling::div//span//text()"
        ]
      },
      "total_expense_net": {
        "paths": [
          "//*[contains(text(), 'Net Expense Ratio')]//following-sibling::div//span//text()"
        ]
      },
      "share_inception_date": {
        "paths": [
          "//*[contains(text(), 'Inception date')]//following-sibling::div//span//text()"
        ]
      },
      "effective_duration": {
        "paths": [
          "//*[contains(text(), 'Effective Duration')]//following-sibling::div//span//text()"
        ]
      },
      "effective_duration_date": {
        "paths": [
          "//*[@class='nuv-data-callout__date']//following-sibling::span//text()"
        ]
      },
      "average_weighted_maturity": {
        "paths": [
          "//*[contains(text(), 'Average Effective Maturity')]//following-sibling::div//span//text()"
        ]
      },
      "average_weighted_maturity_as_of_date": {
        "paths": [
          "//*[contains(text(), 'Average Effective Maturity')]//following-sibling::div//*[contains(text(), ' As of')]/span//text()",
          "//h2[starts-with(normalize-space(text()),'Fund characteristics')]//parent::div//span[contains(text(),' As of')]",
          "//h2[starts-with(normalize-space(text()),'Fund characteristics')]//parent::div//*[contains(text(),'As of')]//span//text()"
        ]
      },
      "temp_fund_managers": {
        "paths": [
          "//div[@id='article-author'][1]//h3//text()"
        ],
        "return_type": "list"
      },
      "temp_sec_yield_30": {
        "paths": [
          "//*[contains(text(), '30-Day SEC Yield (Sub)' )]//following-sibling::div"
        ],
        "return_type": "selector",
        "child_return_strategy": "single_item"
      },
      "sec_yield_30_day": {
        "paths": [
          ".//span[@class='fund_symbol']//text()"
        ],
        "selector": "temp_sec_yield_30"
      },
      "sec_yield_date_30_day": {
        "paths": [
          ".//div[@class='nuv-data-callout__item__date']//span//text()"
        ],
        "selector": "temp_sec_yield_30"
      },
      "temp_sec_yield_without_waivers_30_day": {
        "paths": [
          "//*[contains(text(), '30-Day SEC Yield (Unsub)' )]//following-sibling::div"
        ],
        "return_type": "selector",
        "child_return_strategy": "single_item"
      },
      "sec_yield_without_waivers_30_day": {
        "paths": [
          ".//span[@class='fund_symbol']//text()"
        ],
        "selector": "temp_sec_yield_without_waivers_30_day"
      },
      "sec_yield_without_waivers_date_30_day": {
        "paths": [
          ".//div[@class='nuv-data-callout__item__date']//span//text()"
        ],
        "selector": "temp_sec_yield_without_waivers_30_day"
      },
      "benchmarks": {
        "paths": [
          "//h3[contains(text(),'Performance')]//ancestor::div//tbody//tr//td[1]//span[contains(text(),'Index')]//text()"
        ],
        "cleanup_functions": [
          "list(set(obj.get('benchmarks')))"
        ],
        "return_type": "list"
      },
      "duration": {
        "paths": [
          "//*[contains(text(), 'Option-a')]//following-sibling::div//div[1]//span/text()"
        ]
      },
      "duration_as_of_date": {
        "paths": [
          "//*[contains(text(), 'Option-a')]//following-sibling::div//div[2]//span/text()"
        ]
      }
    }
  }
}
