{
  "website": "www.nuveen.com",
  "spider": "financial_detail",
  "domain": "financial",
  "crawl_method": "SCRAPY_GET",
  "allowed_domains": [
    "www.nuveen.com"
  ],
  "parsing_type": "xpath",
  "country": "US",
  "language": "ENGLISH",
  "listing": {
    "crawl_method": "SCRAPY_GET",
    "parsing_type": "xpath",
    "start_urls": [
      "https://www.nuveen.com/en-us/mutual-funds/nuveen-limited-term-municipal-bond-fund?shareclass=I"
    ]
  },
  "detail": {
    "crawl_method": "SCRAPY_GET",
    "parsing_type": "xpath",
    "ext_codes": {
      "instrument_name": {
        "paths": [
          "//*[@class='nuv-product-detail-hero-retail__main-title no-outline']//text()"
        ]
      },
      "nasdaq_ticker": {
        "paths": [
          "//*[contains(text(), 'Symbol')]//following-sibling::div//span//text()"
        ]
      },
      "cusip": {
        "paths": [
          "//*[contains(text(), 'CUSIP')]//following-sibling::div//span//text()"
        ]
      },
      "portfolio_assets": {
        "paths": [
          "//*[contains(text(), 'Fund Net Assets')]//following-sibling::div//span//text()"
        ]
      },
      "portfolio_assets_date": {
        "paths": [
		  "//*[@class='nuv-data-callout__date']//following-sibling::span//text()"
		]
      },
      "total_expense_gross": {
        "paths": [
          "//*[contains(text(), 'Gross Expense Ratio')]//following-sibling::div//span//text()"
        ]
      },
      "total_expense_net": {
        "paths": [
          "//*[contains(text(), 'Net Expense Ratio')]//following-sibling::div//span//text()"
        ]
      },
      "share_inception_date": {
        "paths": [
          "//*[contains(text(), 'Inception date')]//following-sibling::div//span//text()"
        ]
      },
      "average_effective_duration": {
        "paths": [
          "//*[contains(text(), 'Effective Duration')]//following-sibling::div//span//text()"
        ]
      },
      "average_effective_duration_as_of_date": {
        "paths": [
          "//*[@class='nuv-data-callout__date']//following-sibling::span//text()"
        ]
      },
      "average_weighted_maturity": {
        "paths": [
          "//*[contains(text(), 'Average Effective Maturity')]//following-sibling::div//span//text()"
        ]
      },
      "average_weighted_maturity_as_of_date": {
        "paths": [
          "//*[@class='nuv-data-callout__date']//following-sibling::span//text()"
        ]
      },
      "temp_fund_managers": {
        "paths": [
          "//div[@class='nuv-article-author__profile']"
        ],
        "return_type": "selector"
      },
	  "fund_manager":{
	   "paths":["//h3//text()"
	   ],
	   "selector":"temp_fund_managers"
	   
	   }
    },
    "navigation": {
    }
  }
}