{
    "website": "https://www.marsicofunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "marsicofunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.marsicofunds.com//funds/individual/fundid_1/focus-fund/overview.fs",
            "https://www.marsicofunds.com//funds/individual/fundid_2/growth-fund/overview.fs",
            "https://www.marsicofunds.com//funds/individual/fundid_3/21st-century-fund/overview.fs",
            "https://www.marsicofunds.com//funds/individual/fundid_4/international-opportunities-fund/overview.fs",
            "https://www.marsicofunds.com//funds/individual/fundid_6/global-fund/overview.fs"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h2[@class='fund-page-title mb-0']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//th[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//th[contains(text(),'CUSIP')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//th[contains(text(),'Fund Assets')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]/small/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace('as of', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//th[contains(text(),'Distribution and Service (12b-1) Fees')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "other_expenses": {
                "paths": [
                    "//th[contains(text(),'Other Expenses')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//th[contains(text(),'Management Fee')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "expense_waivers": {
                "paths": [
                    "//th[contains(text(),'Fee Waiver and/or Expense Reimbursement')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Total Annual Fund Operating Expenses')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//th[contains(text(),'Net Expenses')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//th[contains(text(),'Inception')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Regular Account')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//a[contains(text(),'Manager Bios')]/parent::li/parent::ul/parent::div/following-sibling::div/div/h2"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}