{
  "website": "www.pgim.com",
  "spider": "financial_detail_pgim_com",
  "domain": "financial",
  "crawl_method": "SCRAPY_GET",
  "allowed_domains": [
    "www.pgim.com"
  ],
  "parsing_type": "xpath",
  "country": "US",
  "language": "ENGLISH",
  "listing": {
    "crawl_method": "SCRAPY_GET",
    "parsing_type": "jpath",
    "start_urls": [
      "https://www.pgim.com/pcom6/services/pcom/reportjson?pageid=3"
    ],
    "ext_codes": {
      "temp_blocks": {
        "paths": [
          "['Funds'][*]"
        ],
        "child_return_strategy": "multiple_items",
        "return_type": "selector-json"
      },
      "fund_url": {
        "paths": [
          "['HyphenatedFundName']"
        ],
        "selector": "temp_blocks",
        "cleanup_functions": [
          "'https://www.pgim.com/investments/mutual-funds/' + obj.get('fund_url')"
        ]
      },
      "instrument_name": {
        "paths": [
          "['FundName']"
        ],
        "selector": "temp_blocks"
      }
    }

  },
  "detail": {
    "crawl_method": "SELENIUM",
    "parsing_type": "xpath",
    "wait_time": 8,
    "iframe": "//iframe[@title='pcomTitle']",
    "ext_codes": {
      "portfolio_management_style": {
        "paths": [
          "//strong[contains(text(), 'Style:')]/following-sibling::text()[1]"
        ]
      },
      "portfolio_assets": {
        "paths": [
          "//strong[contains(text(), 'Total Net Assets:')]/following-sibling::text()[1]"
        ]
      },
      "investment_objective": {
        "paths": [
          "(//div[contains(text(), 'Objective')])[last()]/following-sibling::p/text()",
          "(//div[contains(text(), 'Objective')])/following-sibling::p/text()"
        ],
        "selector": "root",
        "return_type": "str",
        "parsing_type": "xpath"
      },
      "date": {
        "paths": ["//span[@class='h3-date']/text()"],
        "selector": "root1"
      },
      "temp_class_blocks": {
        "paths": [
          "//span[text()='Fund ID']/parent::th/parent::tr/parent::thead/parent::table/tbody/tr"
        ],
        "return_type": "selector",
        "child_return_strategy": "multiple_items"
      },
      "share_class": {
        "paths": [
          ".//td[1]//text()"
        ],
        "selector": "temp_class_blocks"
      },
      "cusip": {
        "paths": [
          ".//td[3]//text()"
        ],
        "selector": "temp_class_blocks"
      },
      "nasdaq_ticker": {
        "paths": [
          ".//td[4]//text()"
        ],
        "selector": "temp_class_blocks"
      },
      "average_weighted_maturity": {
        "paths": [
          "//*[contains(text(), 'Average Maturity')]/parent::span/following-sibling::span//text()"
        ]
      },
      "duration": {
        "paths": [
          "//*[contains(text(), 'Duration')]/parent::span/following-sibling::span//text()"
        ]
      },
      "duration_as_of_date": {
        "paths": [
          "//*[contains(text(), 'Fund Statistics')]/small/text()"
        ],
        "cleanup_functions": [
          "obj['duration_as_of_date'].replace('as of', '')"
        ]
      },
      "average_weighted_maturity_as_of_date": {
        "paths": [
          "//*[contains(text(), 'Fund Statistics')]/small/text()"
        ],
        "cleanup_functions": [
          "obj['duration_as_of_date'].replace('as of', '')"
        ]
      },
      "turnover_rate": {
        "paths": [
          "//*[contains(text(), 'Turnover Ratio')]/parent::span/following-sibling::span//text()"
        ]
      },
      "turnover_rate_date": {
        "paths": [
          "//*[contains(text(), 'Fund Statistics')]/small/text()"
        ],
        "cleanup_functions": [
          "obj['duration_as_of_date'].replace('as of', '')"
        ]
      }
    },
    "navigation": {
    }
  }
}