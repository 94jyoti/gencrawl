{
    "website": "http://www.bridgebuildermutualfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "bridgebuildermutualfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "http://www.bridgebuildermutualfunds.com/fund-listing/bridge-builder-core-plus-bond-fund/",
            "http://www.bridgebuildermutualfunds.com/fund-listing/bridge-builder-municipal-bond-fund/",
            "http://www.bridgebuildermutualfunds.com/fund-listing/bridge-builder-large-cap-growth-fund/",
            "http://www.bridgebuildermutualfunds.com/fund-listing/bridge-builder-large-cap-value-fund/",
            "http://www.bridgebuildermutualfunds.com/fund-listing/bridge-builder-small-mid-cap-growth-fund/",
            "http://www.bridgebuildermutualfunds.com/fund-listing/bridge-builder-small-mid-cap-value-fund/",
            "http://www.bridgebuildermutualfunds.com/fund-listing/bridge-builder-international-equity-fund/",
            "http://www.bridgebuildermutualfunds.com/fund-listing/bridge-builder-core-bond-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "(//h4)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//*[text()='Ticker:']/following::text()[1]"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//*[text()='CUSIP:']/following::text()[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//*[text()='Total Assets:']/following::text()[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//*[text()='Gross Expense Ratio:']/following::text()[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//*[text()='Net Expense Ratio*:']/following::text()[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//*[text()='Inception Date:']/following::text()[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//*[text()='Objective']/following-sibling::p[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//*[text()='Strategy']/following-sibling::p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "sub_advisor": {
                "paths": [
                    "//div[@aria-labelledby=\"fusion-tab-investmentsub-advisers\"]//h3/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "(//div[@aria-labelledby=\"fusion-tab-performance\"]//table)[1]//tbody//tr[position() >1]//td[1]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}