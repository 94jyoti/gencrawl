{
    "website": "https://clipperfund.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "clipperfund.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
         "spider": "financial_detail_clipperfund_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://clipperfund.com/clipper/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class='column fullCol fundPage']/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Expense Ratio')]/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h3[contains(text(),'Investment Strategy')]/following-sibling::p[5]/font/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//td[contains(text(),'Turnover Rate')]/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h2[contains(text(),'Portfolio Characteristic')]/span/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}