{
    "website": "https://www.ftportfolios.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "ftportfolios.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_ftportfolios_com",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.ftportfolios.com/Retail/MF/MFSummary.aspx?Ticker=FPEAX"
            
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//span[@id='FundNavigation_lblPageHeader']/text()",
                    "//span[@id='FundNavigation1_lblPageHeader']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//td[contains(text(),'Share Class')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_consultant": {
                "paths": [
                    "//td[contains(text(),'Servicing Agent')]/following-sibling::td/text()",
                    "//td[contains(text(),'Sub-Advisor')]/following-sibling::td/text()"
