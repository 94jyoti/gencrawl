                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets": {
                "paths": [
                    "//td[contains(text(),'Total Net Assets')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//div[@id='CurrentFundData1_FundControlContainer']/div[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['total_net_assets_date'].split('as of')[-1]",
                    "obj['total_net_assets_date'].replace(')',' ')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_shares_outstanding": {
                "paths": [
                    "//td[contains(text(),'Outstanding Shares')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_shares_outstanding_date": {
                "paths": [
                    "//div[@id='CurrentFundData1_FundControlContainer']/div[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['total_shares_outstanding_date'].split('as of')[-1]",
                    "obj['total_shares_outstanding_date'].replace(')',' ')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "deferred_sales_charge": {
                "paths": [
                    "//td[contains(text(),' Deferred Sales Charge')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//td[contains(text(),'Maximum Initial Sales Charge')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Gross Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Net Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
