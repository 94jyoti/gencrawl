            "minimum_additional_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Subsequent Investment')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//td[contains(text(),'Dividend Frequency')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "(//td[contains(text(),'30-Day SEC')]/following-sibling::td)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "(//td[contains(text(),'30-Day SEC Yield ')]/span)[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_date_30_day'].split('as of')[-1]",
                    "obj['sec_yield_date_30_day'].replace(')',' ')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_30_day": {
                "paths": [
                    "//td[contains(text(),'Unsubsidized 30-Day SEC')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_date_30_day": {
                "paths": [
                    "(//td[contains(text(),'30-Day SEC Yield ')]/span)[2]/text()"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_without_waivers_date_30_day'].split('as of')[-1]",
                    "obj['sec_yield_without_waivers_date_30_day'].replace(')',' ')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//div[contains(text(),'Index Performance')]/parent::td/parent::tr/following-sibling::tr/td[1][contains(text(),'Index')]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            }
        },
        "pagination": {
            "pagination": {
                "paths": [
                    "//b[contains(text(),'Share Classes')]/parent::td/parent::tr/following-sibling::tr/td/a/@href"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}