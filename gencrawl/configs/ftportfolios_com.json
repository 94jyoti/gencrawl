                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//b[contains(text(),'Investment Objective/Strategy')]/parent::div/span/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//b[contains(text(),'Investment Objective/Strategy')]/parent::div/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_modified_duration": {
                "paths": [
