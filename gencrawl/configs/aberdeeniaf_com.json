{
    "website": "https://www.aberdeeniaf.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "aberdeeniaf.com","aberdeenstandard.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.aberdeenstandard.com/en-us/us/investor/fund-details/aberdeen-china-a-share-equity-fund/class-a/us0030191060"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[contains(text(),'Fund')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/parent::tr//td/strong"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/parent::tr//td/strong/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//div[contains(text(),'Shareclass')]/following-sibling::div/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "isin": {
                "paths": [
                    "//td[contains(text(),'ISIN')]/parent::tr//td/strong/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "asset_class": {
                "paths": [
                    "//td[contains(text(),'Asset Class')]/parent::tr//td/strong/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_consultant": {
                "paths": [
                    "//td[contains(text(),'Fund advisory company')]/parent::tr//td/strong/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Asset Under Management (AUM)')]/parent::tr//td/strong/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "(//td[contains(text(),'Gross Expense Ratio')]/parent::tr//td/strong)/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'NET Expense Ratio')]/parent::tr//td/strong/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Launch date')]/parent::tr//td/strong/text()                                                                                           "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//div[@class='fund__description']/p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum investment')]/parent::tr//td/strong/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//td[contains(text(),'Benchmark')]/parent::tr//td/strong/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}