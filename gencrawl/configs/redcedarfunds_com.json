{
    "website": "https://redcedarfunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "redcedarfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://redcedarfunds.com/short-term-bond-fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[contains(text(),'Fund')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//p[contains(text(),'Ticker')]/following-sibling::h4/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//span[contains(text(),'cusip')]/parent::p/following-sibling::h4/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_consultant": {
                "paths": [
                    "//em[contains(text(),'Investment Management')]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_consultant'].split('LLC')[0] +'LLC'"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//span[contains(text(),'Expense Ratio')]/parent::p/following-sibling::h4/text()[contains(.,'Gross')]"
                ],
                "cleanup_functions": [
                    "obj['total_expense_gross'].split(':')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//span[contains(text(),'Expense Ratio')]/parent::p/following-sibling::h4/text()[contains(.,'Net')]"
                ],
                "cleanup_functions": [
                    "obj['total_expense_net'].split(':')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//span[contains(text(),'Inception date')]/parent::p/following-sibling::h4/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h1[contains(text(),'Fund')]/following-sibling::p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//strong[contains(text(),'Minimum Initial Investment')]/following-sibling::span[1]/text()"
                ],
                "cleanup_functions": [
                    "\"$\" + obj['minimum_initial_investment'].split('$')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//strong[contains(text(),'Minimum Additional Investment')]/following-sibling::span[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['minimum_additional_investment'].split('minimum of')[-1].split('for')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//span[contains(text(),'INDEX')]/parent::p/following-sibling::h4/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//a[contains(text(),'Read Bio')]/ancestor::div[1]/h3"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}