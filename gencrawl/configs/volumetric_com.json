{
    "website": "https://www.volumetric.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "volumetric.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_volumetric_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.volumetric.com/general-info"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//span[contains(text(),'Ticker Symbol')]/parent::span/parent::h2/parent::div/following-sibling::div[@id='comp-jbty04ho']/p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//span[contains(text(),'Ticker Symbol')]/parent::span/parent::h2/parent::div/following-sibling::div[@id='comp-jbty180o']/p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_consultant": {
                "paths": [
                    "//span[contains(text(),'Investment Adviser')]/parent::span/parent::h2/parent::div/following-sibling::div[@id='comp-jbty5pyk']/p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//span[contains(text(),'Objective of Fund')]/parent::h2/parent::div/following-sibling::div[@id='comp-jbtxdvlt']/p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//span[contains(text(),'Account Minimums')]/parent::span/parent::h2/parent::div/following-sibling::div[@id='comp-jbtxmyka']/ul/li[1]/p//text()"
                ],
                "cleanup_functions": [
                    "obj['minimum_initial_investment'].split(':')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//span[contains(text(),'Account Minimums')]/parent::span/parent::h2/parent::div/following-sibling::div[@id='comp-jbtxmyka']/ul/li[2]/p//text()"
                ],
                "cleanup_functions": [
                    "obj['minimum_additional_investment'].split(':')[-1]",
                    " obj['minimum_additional_investment'].split('(')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}