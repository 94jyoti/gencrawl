{
    "website": "https://www.leadercapital.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "leadercapital.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.leadercapital.com/mutual-funds/leader-short-duration-bond-fund/",
            "https://www.leadercapital.com/mutual-funds/leader-total-return-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "(//span[text()='Yield']//following::table)[1]//tr//td[1]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split('(')[-1].replace(\")\",\"\")"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "(//span[text()='Yield']//following::table)[1]//tr//td[1]//text()"
                ],
                "cleanup_functions": [
                    "[i.split('(')[0].strip() for i in obj['share_class']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "asset_class": {
                "paths": [
                    "//td[contains(.,'Asset Class')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(.,'Size of Fund')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "(//span[text()='Fees']//following::table)[1]//tr//td[1][contains(text(),'(')]/following-sibling::td"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//span[text()='Fees']//following::table[1]//tr//td[1][contains(text(),'Management Fee')]/following-sibling::td"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//td[contains(.,'Distribution Frequency')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "(//span[text()='Yield']//following::table)[1]//tr//td[1]//following-sibling::td"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//td[contains(.,'Benchmark Index')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}