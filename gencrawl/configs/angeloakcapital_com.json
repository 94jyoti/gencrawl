{
    "website": "https://angeloakcapital.com",
    "spider": "financial_detail",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "angeloakcapital.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "crawl_method": "SELENIUM",
        "wait_time": 5,
        "parsing_type": "xpath",
        "start_urls": [
            "https://angeloakcapital.com/investments/anfix/ https://angeloakcapital.com/investments/anflx/ https://angeloakcapital.com/investments/aflcx/ https://angeloakcapital.com/investments/anhix/ https://angeloakcapital.com/investments/anhax/ https://angeloakcapital.com/investments/angix/ https://angeloakcapital.com/investments/anglx/ https://angeloakcapital.com/investments/angcx/ https://angeloakcapital.com/investments/aouix/ https://angeloakcapital.com/investments/aouax/"
        ],
        "ext_codes": {
            "share_class": {
                "paths": [
                    "(//select[@class = 'share_class']/option[@selected])/text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('-')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "(//select[@class = 'share_class']/option[@selected])/text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split('-')[1]"
                ],
                "return_type": "str",
                "selector": "root",
                "comment": "part of the X-Path   A - ANGLX\nHere we only need ANGLX"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class = 'team_bio_info']/h4"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "benchmarks": {
                "paths": [
                    "//tr[@class= 'index']/th/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Gross Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//th[contains(text(),'Net Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h4[contains(text(),'Objective')]/following-sibling::p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//th[contains(text(),'Fund Assets')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h3[contains(text(),'Fund Characteristics')]/following-sibling::p/small/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//td[@id = 'sec_subsidized']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "//small[@id = 'sec_date']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_30_day": {
                "paths": [
                    "//td[@id = 'sec_unsubsidized']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_date_30_day": {
                "paths": [
                    "//small[@id = 'sec_date']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "distribution_yield_12_month": {
                "paths": [
                    "//th[contains(text(),'Distribution Yield')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration": {
                "paths": [
                    "//th[contains(text(),'Effective Duration')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration_date": {
                "paths": [
                    "//h3[contains(text(),'Fund Characteristics')]/following-sibling::p/small/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}