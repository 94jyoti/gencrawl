{
    "website": "https://bdrex.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "bdrex.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://bdrex.com/investment-opportunities/broadstone-real-estate-access-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//header[@class='entry-header']/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//th[contains(text(),'Class')]/parent::tr/parent::thead/following-sibling::tbody/tr/td[1]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector" 	
            },
            "nasdaq_ticker": {
                "paths": [
                    "//th[contains(text(),'Symbol')]/parent::tr/parent::thead/following-sibling::tbody/tr/td[2]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//th[contains(text(),'CUSIP')]/parent::tr/parent::thead/following-sibling::tbody/tr/td[3]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_consultant": {
                "paths": [
                    "//h2[contains(text(),'Portfolio Managers')]/following-sibling::p[7]/following-sibling::h2/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_consultant'].split(':')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//td[contains(text(),'Sales Load')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h2[contains(text(),'Fund Objective')]/parent::div/p[3]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Initial Investment')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//h3[contains(text(),'Distributions')]/parent::div/div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//td[contains(text(),'Dividend Frequency')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            }
        }
    }
}