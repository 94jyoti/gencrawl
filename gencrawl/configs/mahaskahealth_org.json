{
    "website": "https://www.mahaskahealth.org/",
    "domain": "hospital",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "mahaskahealth.org"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.mahaskahealth.org/blog/providers/jessica-tokle-arnp/"
        ],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "speciality": {
                "paths": [
                    "//strong[contains(text(),'Specialty')]/following-sibling::a"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "address_raw": {
                "paths": [
                    "//div[@class='widget-info']"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            }
        }
    }
}