{
    "website": "https://kaynefunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "kaynefunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://kaynefunds.com/karix/",
            "https://kaynefunds.com/karrx/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[contains(text(),'Fund')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'CUSIP')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'Share class')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'Total Assets')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]"
                ],
                "cleanup_functions": [
                    "obj['total_net_assets_date'].split('of ')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "deferred_sales_charge": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'Maximum deferred sales load')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'Redemption Fee')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'Maximum front-end sales load')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'Gross Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'Net Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'Inception date')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h2[contains(text(),'Objective')]//following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h2[contains(text(),'Investment Strategy')]//following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//h2[contains(text(),'Fund Facts')]//following::th[contains(text(),'Minimum Investment')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}