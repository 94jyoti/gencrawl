{
  "website": "www.americanbeaconfunds.com",
  "spider": "financial_detail",
  "domain": "financial",
  "crawl_method": "SCRAPY_GET",
  "allowed_domains": [
    "www.americanbeaconfunds.com"
  ],
  "parsing_type": "xpath",
  "country": "US",
  "language": "ENGLISH",
  "listing": {
    "crawl_method": "SCRAPY_GET",
    "parsing_type": "xpath",
    "start_urls": [
      "https://www.americanbeaconfunds.com/mutual_funds/TwentyFourShortTerm.aspx"
    ]
  },
  "detail": {
    "crawl_method": "SCRAPY_GET",
    "parsing_type": "xpath",
    "ext_codes": {
      "investment_objective": {
        "paths": [
          "//*[contains(text(), 'Investment Objective')]//following-sibling::p//text()"
        ],
        "return_type":"join"
      },
      "instrument_name": {
        "paths": [
          "//div[@id='main']//h2//text()"
        ],
      "return_type":"join"
      },
      "temp_item_class": {
        "paths": [
          "//table[@id='grdSymbols']//tr[position()>1]"
        ],
        "return_type": "selector",
        "child_return_strategy": "multiple_items"
      },
      "share_class": {
        "paths": [
          "./td[1]/text()"
        ],
        "selector": "temp_item_class"
      },
      "nasdaq_ticker": {
        "paths": [
          "./td[2]/text()"
        ],
        "selector": "temp_item_class"
      },
      "effective_duration_date":{
        "paths":["//*[contains(text(), 'Portfolio info')]//span//text()"]

      },
      "temp_class_returns": {
        "paths": [
          "//*[contains(text(), 'TOTAL RETURNS')]//following-sibling::table//tr[position()>1]"
        ],
        "return_type": "selector",
        "child_return_strategy":"multiple_objects"
      },
      "share_class_1":{
        "paths":[".//td[1]/text()"],
        "selector":"temp_class_returns"
      },
      "inception_date":{
        "paths":[".//td[2]/text()"],
        "selector":"temp_class_returns"
      },
      "cusip":{
        "paths":[".//td[3]/text()"],
        "selector":"temp_class_returns"
      },
      "temp_class_benchmarks":{
        "paths":[
          "//table[@id='grdBenchmarks']"
        ],
        "return_type": "selector",
        "child_return_strategy": "multiple_objects"
      },
      "benchmarks":{
        "paths":[".//tr[position() >1]//td[1]//text()"],
        "selector":"temp_class_benchmarks"
      },
      "temp_class_expense_ratios":{
        "paths":[
          "//*[contains(text(), 'Expense Ratios')]//following-sibling::div[1]//table//tr[position()>1]"
        ],
        "return_type":"selector",
        "child_return_strategy":"multiple_objects"
       },
       "expense_ratios_share_class":{
        "paths":[".//td[1]//text()"],
        "selector":"temp_class_expense_ratios"
       },
       "expense_ratios_gross":{
        "paths":[".//td[2]//span//text()"],
        "selector":"temp_class_expense_ratios"
       },
       "expense_ratios_net":{
        "paths":[".//td[3]//span//text()"],
        "selector":"temp_class_expense_ratios"
       },
       "temp_sec_30":{
        "paths":[
          "//*[contains(text(), 'SEC 30-Day')]//following-sibling::div//table//tr[position()>1]"
        ],
        "return_type":"selector",
        "child_return_strategy":"multiple_objects"
       },
       "sec_30_share_class":{
        "paths":[".//td[1]//text()"],
        "selector":"temp_sec_30"
       },
       "sec_30_actual":{
        "paths":[".//td[2]//span//text()"],
        "selector":"temp_sec_30"
       },
       "sec_30_unsubsized":{
        "paths":[".//td[3]//span//text()"],
        "selector":"temp_sec_30"
       },
       "effective_duration":{
        "paths":["//*[contains(text(), 'Effective Duration')]//following-sibling::td//text()"]
       },
       "average_weighted_maturity":{
        "paths":["//*[contains(text(), 'Effective Maturity')]//following-sibling::td//text()"]
       },
       "temp_fund_managers":{
       "paths":["//strong[contains(text(), 'Portfolio  Managers')]//parent::p//following-sibling::ul[position()=1]//li//text()"],
       "return_type":"list"
       }
       
    }
      }
    }
