{
    "website": "https://www.homesteadfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "homesteadfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.homesteadfunds.com/our-funds/daily-income-fund/",
            "https://www.homesteadfunds.com/our-funds/short-term-government-securities-fund/",
            "https://www.homesteadfunds.com/our-funds/short-term-bond-fund/",
            "https://www.homesteadfunds.com/our-funds/intermediate-bond-fund/",
            "https://www.homesteadfunds.com/rural-america-growth-income-fund/",
            "https://www.homesteadfunds.com/our-funds/stock-index-fund/",
            "https://www.homesteadfunds.com/our-funds/value-fund/",
            "https://www.homesteadfunds.com/our-funds/growth-fund/",
            "https://www.homesteadfunds.com/our-funds/international-equity-fund/",
            "https://www.homesteadfunds.com/our-funds/small-company-stock-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[@class='page-title']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[@class='ticker-symbol']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//th[contains(text(),'CUSIP Number')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "asset_class": {
                "paths": [
                    "//th[contains(text(),'Asset Allocation')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Net Assets')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//section[@class='portfolio-characteristics section']//span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "expense_waivers": {
                "paths": [
                    "//th[contains(text(),'Fee Waiver')]/following-sibling::td"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Expense Ratio')][not(contains(.,'Median'))]/following-sibling::td"
                ],
                "cleanup_functions": [
                    "obj['total_expense_gross'].split('(')[0].strip()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//th[contains(text(),'Expense Ratio')][not(contains(.,'Median'))]/following-sibling::td"
                ],
                "cleanup_functions": [
                    "obj['total_expense_net'].split('(')[1].replace('Net', '').replace(')', '').strip()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//th[contains(text(),'Inception')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "duration": {
                "paths": [
                    "//td[contains(text(),'Duration')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "duration_as_of_date": {
                "paths": [
                    "(//span[@class='date'])[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity": {
                "paths": [
                    "//td[contains(text(),'Weighted Average Maturity')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity_as_of_date": {
                "paths": [
                    "(//span[@class='date'])[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//th[contains(text(),'30-Day SEC Yield')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "(//span[@class='date'])[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_7_day": {
                "paths": [
                    "//th[contains(text(),'7-Day Effective Yield')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_7_day": {
                "paths": [
                    "(//span[@class='date'])[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//th[contains(text(),'Benchmark')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class='manager-container']//a/h5",
                    "//section[@class='folio']//h3"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['fund_manager'].split('(')[0] "
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}