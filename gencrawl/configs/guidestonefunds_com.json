{
    "website": "https://www.guidestonefunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "guidestonefunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.guidestonefunds.com/Funds/DefensiveMarketStrategies?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/EquityIndex?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/ValueEquity?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/GrowthEquity?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/SmallCapEquity?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/InternationalEquity?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/EmergingMarketsEquity?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/MoneyMarket?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/LowDurationBond?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/MediumDurationBond?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/GlobalBond?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/GlobalRealEstateSecurities?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/StrategicAlternatives?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/GlobalImpact?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/MyDestination2015?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/MyDestination2025?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/MyDestination2035?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/MyDestination2045?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/MyDestination2055?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/ConservativeAllocation?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/BalancedAllocation?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/GrowthAllocation?sclass=Investor",
            "https://www.guidestonefunds.com/Funds/AggressiveAllocation?sclass=Investor"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//p[@id=\"expandable\"]/text()"
                ],
                "cleanup_functions": [
                    "obj['instrument_name'].split('/')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//strong[contains(text(),'NASDAQ')]/following-sibling::text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//p[@class='text-fund-data-date']/a/strong/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//strong[contains(text(),'Fund Size ($ in millions)')]/following::td[@class=\"label-cell\"][1]/following::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//strong[contains(text(),'FEES AND EXPENSES (annualized)')]/following::td[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//strong[contains(text(),'FEES AND EXPENSES (annualized)')]/following::td[4]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//strong[contains(text(),'Inception Date')]/following-sibling::text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//*[text()='Investment Objective:']/following::p[1][string-length(text()) >= 1]/text()",
                    "//*[text()='Investment Objective:']/following::p[2]/span//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//*[text()='Investment Strategy:']/following::p[1][string-length(text()) >= 1]/text()",
                    "//*[text()='Investment Strategy:']/following::p[2]/span//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration": {
                "paths": [
                    "//*[contains(text(),'Effective Duration')]/following-sibling::td"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration_date": {
                "paths": [
                    "//strong[contains(text(),'Portfolio Characteristics')]/text()"
                ],
                "cleanup_functions": [
                    "obj['effective_duration_date'].split('as of')[-1].replace(')','')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity": {
                "paths": [
                    "//*[contains(text(),'Weighted Avg Maturity')]/following-sibling::td"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity_as_of_date": {
                "paths": [
                    "//strong[contains(text(),'Portfolio Characteristics')]/text()"
                ],
                "cleanup_functions": [
                    "obj['average_weighted_maturity_as_of_date'].split('as of')[-1].replace(')','')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sub_advisor": {
                "paths": [
                    "//strong[contains(text(),'Sub-adviser Composite')]/following::table[1]//td[@class=\"label-cell\"]"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "(//td[contains(text(),'SEC 30-Day Yield')]/following::td)[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//strong[contains(text(),'Benchmark^')]/following::table[1]//td[@class=\"label-cell\"]"
                ],
                "return_type": "list",
                "selector": "root"
            }
        },
        "pagination": {
            "pagination": {
                "paths": [
                    "//p[@class='text-fund-data-date']/a/@href"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}