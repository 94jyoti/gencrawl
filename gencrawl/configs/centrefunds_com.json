{
    "website": "https://www.centrefunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "centrefunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.centrefunds.com/dhamx-dhanx",
            "https://www.centrefunds.com/dhivx-dhinx"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[@class = 'hero-short__title hero-short__title--fund']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//li[@class = 'tabs__list-item']/a"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_consultant": {
                "paths": [
                    "//p[contains(text(),'investment adviser')]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_consultant'].split('(')[0]",
                    "obj['portfolio_consultant'].split('Fund,')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Gross Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Net Expense Ratio')]/following-sibling::td/span/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h4[contains(text(),'Investment Objective')]/following-sibling::p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "(//td[contains(text(),'Index')])/text()"
                ],
                "cleanup_functions": [
                    "list(set(obj['benchmarks']))"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}