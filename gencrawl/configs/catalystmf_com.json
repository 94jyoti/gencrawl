{
    "website": "https://catalystmf.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "catalystmf.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://catalystmf.com/funds/catalyst-buffered-shield-fund/",
            "https://catalystmf.com/funds/catalyst-multi-strategy-fund/",
            "https://catalystmf.com/funds/catalyst-systematic-alpha-fund/",
            "https://catalystmf.com/funds/catalyst-millburn-dynamic-commodity-strategy-fund/",
            "https://catalystmf.com/funds/catalyst-millburn-hedge-strategy-fund/",
            "https://catalystmf.com/funds/catalyst-teza-algorithmic-allocation-fund/",
            "https://catalystmf.com/funds/catalyst-warrington-strategic-program-fund/",
            "https://catalystmf.com/funds/eavol-nasdaq-100-volatility-overlay-fund/",
            "https://catalystmf.com/funds/catalyst-dynamic-alpha-fund/",
            "https://catalystmf.com/funds/catalyst-insider-buying-fund/",
            "https://catalystmf.com/funds/catalyst-mlp-infrastructure-fund/",
            "https://catalystmf.com/funds/catalyst-pivotal-growth-fund/",
            "https://catalystmf.com/funds/catalyst-lyons-tactical-allocation-fund/",
            "https://catalystmf.com/funds/catalyst-map-global-equity-fund/",
            "https://catalystmf.com/funds/catalyst-enhanced-income-strategy-fund/",
            "https://catalystmf.com/funds/catalyst-insider-income-fund/",
            "https://catalystmf.com/funds/catalyst-floating-rate-income-fund/",
            "https://catalystmf.com/funds/catalyst-map-global-balanced-fund/",
            "https://catalystmf.com/funds/catalyst-smh-high-income-fund/",
            "https://catalystmf.com/funds/catalyst-smh-total-return-income-fund/",
            "https://catalystmf.com/funds/catalyst-stone-beach-income-opportunity-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class = 'mk-grid']/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//li/a[contains(text(),'Share')]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//strong[contains(text(),'Ticker')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//strong[contains(text(),'CUSIP')]/parent::td/following-sibling::td[not(position()= 4)]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_consultant": {
                "paths": [
                    "//strong[contains(text(),'Investment Advisor')]/parent::p/text()","//span[@class='content-box-heading']//i/following-sibling::text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Prospectus Gross Expense Ratio')]/following-sibling::td[not(position()= 4)]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Prospectus Net Expense Ratio')]/following-sibling::td[not(position()= 4)]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//strong[contains(text(),'Inception Date')]/parent::td/following-sibling::td[not(position()= 4)]/text()"
                ],
                "cleanup_functions": [
                    "[i.split(\"T\")[0] for i in obj['share_inception_date']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//p[contains(text(),'Fund Objective')]/ancestor::h2/following-sibling::div[4]/div/p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//p[contains(text(),'Investment Strategy')]/ancestor::h2/following-sibling::div/div/p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "sub_advisor": {
                "paths": [
                    "//strong[contains(text(),'Sub-Advisor')]/parent::p/text()","//*[@id='fundmanagement']//div[4]/span/i/following::text()[1]","//strong[contains(text(),'Investment Sub-Advisor')]/parent::p/text()"
                ],
                "cleanup_functions":["obj['sub_advisor'].replace('Investment Sub-Advisor:','').replace(':','').strip()"],
                "return_type": "str",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//strong[contains(text(),'Distribution Frequency:')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//*[contains(text(), 'Portfolio Manager') and contains(text(), '|')]/br",
                    "//*[contains(text(), 'Portfolio Manager') and contains(text(), '|')]",
                    "//*[contains(text(), 'Portfolio Managers')]/following-sibling::br"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./following-sibling::text()",
                    "./text()[not(contains(., ':'))]"
                ],
                "cleanup_functions": [
                    "obj['fund_manager'].replace('\\n','').split('|')[0]"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}