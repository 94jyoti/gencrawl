{
    "website": "http://www.ashmoregroup.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "ashmoregroup.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_ashmore_com",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemcdf-a-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemcdf-c-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-usshort-a-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-usshort-c-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-usigsh-i-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-usigi-i-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-uscdesg-i-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemlcbf-a-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemlcbf-c-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemtrf-a-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemtrf-c-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemscef-a-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemscef-c-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemef-a-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemef-c-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-usfef-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-usfef-c-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-usfef-ins-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-usaf-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-usaf-c-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemefesg-inc-usd",
            "http://www.ashmoregroup.com/us-ii/our-funds/40act-aemefesg-c-inc-usd"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[contains(text(),'Fund')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//strong[contains(text(),'ticker')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//strong[contains(text(),'CUSIP')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//strong[contains(text(),'Share class')  and not(contains(text(),'launch')) and not(contains(text(),'currency'))]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "isin": {
                "paths": [
                    "//strong[contains(text(),'ISIN')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//strong[contains(text(),'Fund size') and not(contains(text(),'as at'))]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//strong[contains(text(),'Fund size as at')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//strong[contains(text(),'launch date') ]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//strong[contains(text(),'Minimum investment')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//strong[contains(text(),'Minimum additional investment')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//strong[contains(text(),'Dividend frequency')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        },
        "pagination": {
            "pagination": {
                "paths": [
                    "//select[@name=\"shareclass\"]//option[not(@selected)]//@value"
                ],
                "cleanup_functions": [
                    "[\"http://www.ashmoregroup.com\"+p for p in obj['pagination']]"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}