        "dchsystem.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.dchsystem.com/find-a-doctor/stephen-e-allen-md/"
        ],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "speciality": {
                "paths": [
                    "//a[contains(@href,'Specialty')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "affiliation": {
                "paths": [
                    "//div[contains(@class,'affiliations')]/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "address_raw": {
                "paths": [
                    "//ul[@class='system-cards items-1']"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            }
        }
    },
    "decision_tags": {
        "practice_in_address": "1"
    }
