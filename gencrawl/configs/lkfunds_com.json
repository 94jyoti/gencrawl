{
    "website": "http://www.lkfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "lkfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "http://www.lkfunds.com/fund/performance.php"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/text()"
                ],
                "cleanup_functions": [
                    "obj['instrument_name'].split('-')[0] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//th[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets": {
                "paths": [
                    "//th[contains(text(),'Total Net Assets')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['total_net_assets']+' Million'"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "(//span[@class='as_of']/text())[3]"
                ],
                "cleanup_functions": [
                    "obj['total_net_assets_date'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//th[contains(text(),'Redemption')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//th[contains(text(),'Sales Charges')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Gross Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//th[contains(text(),'Net Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//th[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//th[contains(text(),'Minimum Investment')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//table[@id='performance']/tbody/tr/td[1][contains(text(),'Index')]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}