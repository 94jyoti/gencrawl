{
    "website": "https://rmbfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "rmbfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://rmbfunds.com/our-funds/rmb-fund",
            "https://rmbfunds.com/our-funds/rmb-mendon-financial-services-fund",
            "https://rmbfunds.com/our-funds/rmb-small-cap-fund",
            "https://rmbfunds.com/our-funds/rmb-smid-cap-fund",
            "https://rmbfunds.com/our-funds/rmb-international-fund",
            "https://rmbfunds.com/our-funds/rmb-japan-fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[@class='page-title']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//button[contains(@class,'switch class')]/p"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//h1[@class='page-title']/following-sibling::p/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//p[contains(text(),'CUSIP')]/preceding-sibling::h3/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "total_net_assets": {
                "paths": [
                    "//p[contains(text(),'AUM as of')]/preceding-sibling::h3/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//p[contains(text(),'AUM as of')]/text()"
                ],
                "cleanup_functions": [
                    "[item.replace('AUM as of','').strip() for item in obj['total_net_assets_date']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//p[contains(text(),'Maximum Sales Charge')]/preceding-sibling::h3/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//p[contains(text(),'Expense Ratio')]/preceding-sibling::h3/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//p[contains(text(),'Inception Date')]/preceding-sibling::h3/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//div[div[p[contains(text(),'Turnover Ratio')]]]/following-sibling::div[1]/p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h2[text()='Portfolio Characteristics']/following-sibling::p[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].replace('As of','').strip()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class='mgr-container']/a[2]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}