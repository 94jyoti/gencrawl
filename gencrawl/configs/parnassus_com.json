{
    "website": "https://www.parnassus.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "parnassus.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.parnassus.com/parnassus-mutual-funds/core-equity/investor-shares",
            "https://www.parnassus.com/parnassus-mutual-funds/mid-cap/investor-shares",
            "https://www.parnassus.com/parnassus-mutual-funds/endeavor/investor-shares",
            "https://www.parnassus.com/parnassus-mutual-funds/mid-cap-growth/investor-shares",
            "https://www.parnassus.com/parnassus-mutual-funds/fixed-income/investor-shares",
            "https://www.parnassus.com/parnassus-mutual-funds/fixed-income/institutional-shares",
            "https://www.parnassus.com/parnassus-mutual-funds/core-equity/institutional-shares",
            "https://www.parnassus.com/parnassus-mutual-funds/mid-cap/institutional-shares",
            "https://www.parnassus.com/parnassus-mutual-funds/endeavor/institutional-shares",
            "https://www.parnassus.com/parnassus-mutual-funds/mid-cap-growth/institutional-shares"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//span[@class = 'carousel--fund-ticker']/parent::h2/text()"
                ],
                "cleanup_functions": [
                    "obj['instrument_name'].split(' In')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//div[contains(text(),'Ticker')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//div[contains(text(),'CUSIP')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//span[@class = 'carousel--fund-ticker']/parent::h2/text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('Fund ')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//div[contains(text(),'Fund Assets')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//small[contains(text(),'As of')]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('of ')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//div[contains(text(),'Gross Expense Ratio')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//div[contains(text(),'Net Expense Ratio')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//div[contains(text(),'Inception Date')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//div[contains(text(),'Fund Objective')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h3[contains(text(),'Fund strategy')]/following-sibling::div/ul/li/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//div[contains(text(),'Minimum Initial Investment')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//div[contains(text(),'Minimum Subsequent Investment')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//td[contains(text(),'Turnover')]/following-sibling::td/span[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h2[contains(text(),'Fund Characteristics')]/parent::div/following-sibling::div/table/thead/tr/th[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].split('of ')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//div[contains(text(),'Distribution Frequency')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//td[contains(text(),'Index')]/text()[contains(.,'Index')]"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h2[contains(text(),'Portfolio Manager')]/following-sibling::div/div/div/div/div/div/div"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}