{
    "website": "https://www.hussmanfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "hussmanfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "wait_time":3,
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.hussmanfunds.com/strategic-growth-fund/",
            "https://www.hussmanfunds.com/strategic-allocation-fund/",
            "https://www.hussmanfunds.com/strategic-total-return-fund/",
            "https://www.hussmanfunds.com/strategic-international-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/span[contains(text(),'Fund')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "annual_fund_operating_expenses_after_fee_waiver": {
                "paths": [
                    "//td[contains(text(),'After Waivers')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Gross Expenses')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Fiscal Year End')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//strong[contains(text(),'INCEPTION')]/parent::span/following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h1[contains(text(),'Objective')]/following-sibling::p[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h1[contains(text(),'Investment Strategy')]/following-sibling::p[count(preceding-sibling::h2)=0]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//h2[contains(text(),'Minimum investment')]/following-sibling::p/text()[contains(.,'Regular')]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//span[contains(text(),'PORTFOLIO MANAGER')]/parent::strong/following-sibling::span"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "list",
                "selector": "temp_fund_managers"
            }
        }
    }
}