{
    "website": "https://www.hardingloevner.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "hardingloevner.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_harding_com",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.hardingloevner.com/ways-to-invest/us-mutual-funds/global-equity-portfolio/",
            "https://www.hardingloevner.com/ways-to-invest/us-mutual-funds/international-equity-portfolio/",
            "https://www.hardingloevner.com/ways-to-invest/us-mutual-funds/emerging-markets-portfolio/",
            "https://www.hardingloevner.com/ways-to-invest/us-mutual-funds/chinese-equity-portfolio/",
            "https://www.hardingloevner.com/ways-to-invest/us-mutual-funds/frontier-emerging-markets-portfolio/",
            "https://www.hardingloevner.com/ways-to-invest/us-mutual-funds/international-small-companies-portfolio/",
            "https://www.hardingloevner.com/ways-to-invest/us-mutual-funds/global-equity-research-portfolio/",
            "https://www.hardingloevner.com/ways-to-invest/us-mutual-funds/international-equity-research-portfolio/",
            "https://www.hardingloevner.com/ways-to-invest/us-mutual-funds/emerging-markets-research-portfolio/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[@class = 'entry-title']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//div[contains(text(),'Ticker')]//preceding-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//div[contains(text(),'CUSIP')]//preceding-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//a[@data-id=\"global-equity-portfolio\"]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//div[contains(text(),'Total Fund Assets')]//preceding-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//p[@class=\"asof_date_facts\"]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//div[contains(text(),'Sales Charge')]//preceding-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//div[contains(text(),'Expense Ratio')]//preceding-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//div[contains(text(),'Inception Date')]//preceding-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//div[contains(text(),'Minimum Investment')]//preceding-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//div[contains(text(),'Turnover')]//preceding-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//p[@class=\"asof_date_facts\"]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//div[contains(text(),'Dividend Policy')]//preceding-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "(//table[@id=\"table-us-mutual-funds-month\"]//following::td[contains(text(),'Index')])[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class='manager']/a[2]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}