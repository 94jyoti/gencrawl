{
    "website": "https://rationalmf.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "rationalmf.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://rationalmf.com/funds/rational-tactical-return-fund/",
            "https://rationalmf.com/funds/rational-resolve-adaptive-asset-allocation-fund/",
            "https://rationalmf.com/funds/rational-pier-88-convertible-securities-fund/",
            "https://rationalmf.com/funds/rational-special-situations-income-fund/",
            "https://rationalmf.com/funds/rational-equity-armor-fund/",
            "https://rationalmf.com/funds/rational-dynamic-brands-fund/",
            "https://rationalmf.com/funds/rational-strategic-allocation-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[contains(text(),'Fund') and not(contains(text(),'('))]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//a[contains(text(),'Share') and not(contains(text(),'Shareholder'))]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/following-sibling::td[not(position() =4)]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//strong[contains(text(),'CUSIP')]/parent::td/following-sibling::td[not(position() =4)]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions":["obj['share_class'].replace('Share','').strip()"],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_consultant": {
                "paths": [
                    "//em[contains(text(),'Rational Advisors')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Prospectus Gross Expense Ratio')]/following-sibling::td[not(position()=4)]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Prospectus Net Expense Ratio')]/following-sibling::td[not(position()=4)]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//strong[contains(text(),'Inception Date')]/parent::td/following-sibling::td[not(position() =4)]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//p[contains(text(),'Fund Objective')]/ancestor::h2/following-sibling::div[4]/div/p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//p[contains(text(),'Investment Strategy')]/ancestor::div[4]/following-sibling::div[position()<5]//p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//strong[contains(text(),'Distribution Frequency')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class = 'content-box-content']"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//p/strong/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}