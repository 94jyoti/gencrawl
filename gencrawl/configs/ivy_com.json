{
  "website": "https://www.ivyinvestments.com/",
  "spider": "financial_detail",
  "domain": "financial",
  "crawl_method": "SCRAPY_GET",
  "parsing_type": "xpath",
  "country": "US",
  "language": "ENGLISH",
  "listing": {
    "crawl_method": "SCRAPY_GET",
    "parsing_type": "xpath",
    "start_urls": [
      "https://www.ivyinvestments.com/reports/ivy"
    ]
  },
  "detail": {
    "crawl_method": "SCRAPY_GET",
	"parsing_type": "xpath",
    "ext_codes": {
      "instrument_name": {
        "paths": [
          "//h3[contains(text(),'Fund Detail')]//parent::header//h2//text()"
        ]
      },
      "nasdaq_ticker": {
        "paths": [
          "//h6[contains(text(),'Fund Facts')]//parent::section//td[contains(text(),'Ticker Symbol')]//following-sibling::td//text()"
        ]
      },
      "cusip": {
        "paths": [
          "//h6[contains(text(),'Fund Facts')]//parent::section//td[contains(text(),'CUSIP')]//following-sibling::td//text()"]
      },
      "share_class": {
        "paths": [
          "//h3[contains(text(),'Fund Detail')]//parent::header//p//text()"
        ],
        "cleanup_functions": [
          "str(obj.get('share_class')).split('//')[-1].replace('shares','')[-1]"],
          "return_type":"str"
      },
      "asset_class": {
        "paths": [
          "//h6[contains(text(),'Fund Facts')]//parent::section//td[contains(text(),'Fund Type')]//following-sibling::td//text()"
        ]
      },
      "portfolio_assets": {
        "paths": [
          "//h6[contains(text(),'Fund Facts')]//parent::section//td[contains(text(),'Fund Assets')]//following-sibling::td//text()"
        ]
      },
      "portfolio_assets_date": {
        "paths": [
          "//h6[contains(text(),'Fund Facts')]//parent::section//td[contains(text(),'Fund Assets')]//span//text()"
        ],
        "cleanup_functions": [
          "str(obj.get('portfolio_assets_date').replace('(','').replace(')',''))"],
          "return_type":"str"
      },
      "total_expense_gross": {
        "paths": [
          "//h3[contains(text(),'Expense Ratios')]//parent::section//td[contains(text(),'Gross')]//following-sibling::td//text()"
        ]
      },
      "total_expense_net": {
        "paths": [
          "//h3[contains(text(),'Expense Ratios')]//parent::section//td[contains(text(),'Net')]//following-sibling::td//text()"
        ]
      },
      "share_inception_date": {
        "paths": [
          "//*[@class='nuv-data-callout__date']//following-sibling::span//text()"
        ]
      },
      "average_weighted_maturity": {
        "paths": [
          "//*[contains(text(), 'Average Effective Maturity')]//following-sibling::div//span//text()"
        ]
      },
      "average_weighted_maturity_as_of_date": {
        "paths": [
          "//*[contains(text(), 'Average Effective Maturity')]//following-sibling::div//*[contains(text(), ' As of')]/span//text()",
          "//h2[starts-with(normalize-space(text()),'Fund characteristics')]//parent::div//span[contains(text(),' As of')]"
        ]
      },
      "temp_fund_managers": {
        "paths": [
          "//div[@id='article-author'][1]//h3//text()"
        ],
        "return_type": "list"
      },
      "temp_sec_yield_30": {
        "paths": [
          "//*[contains(text(), '30-Day SEC Yield (Sub)' )]//following-sibling::div"
        ],
        "return_type": "selector",
        "child_return_strategy": "single_item"
      },
      "sec_yield_30_day": {
        "paths": [
          ".//span[@class='fund_symbol']//text()"
        ],
        "selector": "temp_sec_yield_30"
      },
      "sec_yield_date_30_day": {
        "paths": [
          ".//div[@class='nuv-data-callout__item__date']//span//text()"
        ],
        "selector": "temp_sec_yield_30"
      },
      "temp_sec_yield_without_waivers_30_day": {
        "paths": [
          "//*[contains(text(), '30-Day SEC Yield (Unsub)' )]//following-sibling::div"
        ],
        "return_type": "selector",
        "child_return_strategy": "single_item"
      },
      "sec_yield_without_waivers_30_day": {
        "paths": [
          ".//span[@class='fund_symbol']//text()"
        ],
        "selector": "temp_sec_yield_without_waivers_30_day"
      },
      "sec_yield_without_waivers_date_30_day": {
        "paths": [
          ".//div[@class='nuv-data-callout__item__date']//span//text()"
        ],
        "selector": "temp_sec_yield_without_waivers_30_day"
      },
      "benchmarks": {
        "paths": [
          "//span[@class='nuv-display-style__default' and contains(text(), ' Index')]/text()"
        ],
        "cleanup_functions": [
          "list(set(obj.get('benchmarks')))"
        ],
        "return_type": "list"
      },
      "duration": {
        "paths": [
          "//*[contains(text(), 'Option-a')]//following-sibling::div//div[1]//span/text()"
        ]
      },
      "duration_as_of_date": {
        "paths": [
          "//*[contains(text(), 'Option-a')]//following-sibling::div//div[2]//span/text()"
        ]
      }
    }
  }
}
