{
    "website": "https://www.alger.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "alger.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": ["https://www.alger.com/pages/Products.aspx?productCode=2162", "https://www.alger.com/pages/Products.aspx?productCode=2140", "https://www.alger.com/pages/Products.aspx?productCode=2132"],

        "ext_codes": {
            "retry_condition": {
                "paths": [
                  "//div[@id='main-title']/text()",
                    "//html/@lang"
                ],
                "cleanup_functions": [
                    "True if obj.get('retry_condition') == 'en' else False"
                ]
              },
            "instrument_name": {
                "paths": [
                    "//div[@id='main-title']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//div[@id='main-title']/em/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//li[contains(text(),'CUSIP')]/em/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//li[contains(text(),'Share Class')]/em/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "asset_class": {
                "paths": [
                    "//strong[contains(text(),'Asset Class')]/parent::div/following-sibling::div[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//td[contains(text(),'Service (12b-1) Fees')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "other_expenses": {
                "paths": [
                    "//td[contains(text(),'Other Expenses')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//td[contains(text(),'Management Fees')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "deferred_sales_charge": {
                "paths": [
                    "//td[contains(text(),'Maximum Deferred Sales Charge')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//td[contains(text(),'Maximum Sales Charge')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//span[contains(text(),'Annual Fund Operating Expenses')]/following-sibling::em/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h4[contains(text(),'Investment Goal')]/following-sibling::p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//li[contains(text(),'Minimum Investment')]/em/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//strong[contains(text(),'Primary Benchmark')]/parent::div/following-sibling::div[1]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class='staff-name']/a"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}