{
    "website": "https://www.bmogam.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "bmogam.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.bmogam.com/us-en/individuals/funds/bmo-dividend-income-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-large-cap-growth-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-large-cap-value-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-low-volatility-equity-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-mid-cap-growth-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-mid-cap-value-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-small-cap-growth-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-small-cap-value-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-disciplined-international-equity-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-global-low-volatility-equity-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-lgm-emerging-markets-equity-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-pyrford-international-stock-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-short-term-income-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-strategic-income-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-core-plus-bond-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-corporate-income-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-intermediate-tax-free-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-short-tax-free-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-ultra-short-tax-free-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-government-money-market-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-prime-money-market-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-tax-free-money-market-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-aggressive-allocation-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-balanced-allocation-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-conservative-allocation-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-growth-allocation-fund/",
            "https://www.bmogam.com/us-en/individuals/funds/bmo-moderate-allocation-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[@class=\"title ready\"]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//ul[@class=\"daily-nav-tabs\"]/li"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./a/@title"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split('-')[-1] "
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "./a/@title"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('-')[0] "
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//span[@id=\"fund_assets\"]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//span[@id=\"total_date\"]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//*[contains(text(),'Gross expense ratio')]/preceding-sibling::*[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//*[contains(text(),'Net expense ratio')]/preceding-sibling::*[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//*[contains(text(),'Inception date')]/preceding-sibling::*[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h2[contains(text(),'Objective')]/following::*[normalize-space()][1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//div[contains(@class, 'benchmarks')]//div[@class='benchmark']//text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//*[contains(text(),'Portfolio managers')]/following::div[@class=\"slick-track\"]//h2"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}