{
    "website": "https://www.jamesinvestment.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "jamesinvestment.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_jamesinvestment_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.jamesinvestment.com/fund-template.php?ticker=GLRBX",
            "https://www.jamesinvestment.com/fund-template.php?ticker=JASCX",
            "https://www.jamesinvestment.com/fund-template.php?ticker=JMCRX",
            "https://www.jamesinvestment.com/fund-template.php?ticker=JAVAX"
            
            
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//h2[contains(text(),'Fund Characteristics')]/parent::div/following::td[contains(text(),'Ticker')]/following-sibling::td"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//h2[contains(text(),'Fund Characteristics')]/parent::div/following::td[contains(text(),'Cusip')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//h2[contains(text(),'Fund Characteristics')]/parent::div/following::td[contains(text(),'Net Assets')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h2[contains(text(),'Fund Characteristics')]/parent::div/following::td[contains(text(),'Net Assets')]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('of')[-1].split('(')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//td[contains(text(),'Distribution (12b-1) Fee')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "other_expenses": {
                "paths": [
                    "//td[contains(text(),'Other Expenses')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//td[contains(text(),'Management Fee')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "acquired_fund_fees_and_expenses": {
                "paths": [
                    "//td[contains(text(),'Acquired')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//h2[contains(text(),'Fund Characteristics')]/parent::div/following::td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//*[contains(text(),\"Investment Objective\")]//following::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//*[contains(text(),\"Investment Strategy\")]//following::p[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//h2[contains(text(),'Fund Characteristics')]/parent::div/following::span[contains(text(),'Turnover')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h2[contains(text(),'Fund Characteristics')]/parent::div/following::span[contains(text(),'Turnover')]/parent::td/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].split('End')[-1].split(')')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//strong[contains(text(),'Index') or contains(text(),'Blend')]/text()"
                ],
                "cleanup_functions": [
                    "list(set(obj['benchmarks']))"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}