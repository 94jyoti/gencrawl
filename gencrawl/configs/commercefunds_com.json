{
    "website": "https://www.commercefunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "commercefunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false,
            "CRAWLERA_ENABLED" : false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.commercefunds.com/fund-information/mutual-funds/growth",
            "https://www.commercefunds.com/fund-information/mutual-funds/midcap-growth",
            "https://www.commercefunds.com/fund-information/mutual-funds/value",
            "https://www.commercefunds.com/fund-information/mutual-funds/bond",
            "https://www.commercefunds.com/fund-information/mutual-funds/short-term-government",
            "https://www.commercefunds.com/fund-information/mutual-funds/national-tax-free-intermediate-bond",
            "https://www.commercefunds.com/fund-information/mutual-funds/missouri-tax-free-intermediate-bond",
            "https://www.commercefunds.com/fund-information/mutual-funds/kansas-tax-free-intermediate-bond"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[contains(text(),'Fund')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//th[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//th[contains(text(),'Cusip')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//th[contains(text(),'Total Fund Assets')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//th[contains(text(),'Total Fund Assets')]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('as of ')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//th[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h4[contains(text(),'Objective')]/following-sibling::p/a/span/text()",
                    "//h4[contains(text(),'Objective')]/parent::div/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h4[contains(text(),'Strategy')]/parent::div/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "effective_duration": {
                "paths": [
                    "//th[contains(text(),'Effective Duration')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration_date": {
                "paths": [
                    "//th[contains(text(),'Total Fund Assets')]/text()"
                ],
                "cleanup_functions": [
                    "obj['effective_duration_date'].split('as of ')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//th[contains(text(),'Minimum Initial Investment')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h4[contains(text(),'Fund Manager')]/parent::div"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//p[contains(text(),'CFA')]//text()","./h4/following::text()[contains(.,'CFA')]"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    "./ul/li[2]/text()"
                ],
                "cleanup_functions": [
                    "obj['fund_manager_years_of_experience_in_industry'].split('of')[0]"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_with_fund": {
                "paths": [
                    "./ul/li[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['fund_manager_years_of_experience_with_fund'].split('in')[-1]"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}