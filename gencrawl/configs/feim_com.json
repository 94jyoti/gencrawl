{
    "website": "https://www.feim.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "feim.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.feim.com/financial-professionals/fund/global-fund",
            "https://www.feim.com/financial-professionals/fund/overseas-fund",
            "https://www.feim.com/financial-professionals/fund/us-value-fund",
            "https://www.feim.com/financial-professionals/fund/gold-fund",
            "https://www.feim.com/financial-professionals/fund/global-income-builder-fund",
            "https://www.feim.com/financial-professionals/fund/high-income-fund",
            "https://www.feim.com/financial-professionals/fund/fund-america",
            "https://www.feim.com/financial-professionals/fund/credit-opportunities-fund",
            "https://www.feim.com/financial-professionals/fund/small-cap-opportunity-fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class='block-content']/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//th[text()=' Class' or text()='Class']//ancestor::thead//following-sibling::tbody//tr//td[2][string-length(text()) >= 1]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//th[text()=' Class' or text()='Class']//ancestor::thead//following-sibling::tbody//tr//td[3][string-length(text()) >= 1]"
                ],
                "return_type": "list-map",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "//th[text()=' Class' or text()='Class']//ancestor::thead//following-sibling::tbody//tr//td[1][string-length(text()) >= 1]"
                ],
                "return_type": "list-map",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "(//td[contains(text(),'Total Managed Assets')]/following-sibling::td)[1]/text()",
                    "//td[contains(text(),'Total Net Assets')]/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "(//td[contains(text(),'Total Managed Assets')])[1]/text()",
                    "(//td[contains(text(),'Total Net Assets')])[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace('Total Managed Assets as of ', '')",
                    "obj['portfolio_assets_date'].replace('Total Net Assets as of ', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//td[contains(text(),'Distribution and Service (12b-1) Fees')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "other_expenses": {
                "paths": [
                    "//td[contains(text(),'Other Expenses**')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//td[contains(text(),'Management Fees')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//th[text()='Inception Date' or text()=' Inception Date']//following::tbody//tr//td[4][contains(text(),'/')][string-length(text()) >= 1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Investment')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//td[contains(text(),'Subsequent Investment')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//td[contains(text(),'Turnover')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//td[contains(text(),'Turnover')]/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].replace('Turnover - As of ', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//td[contains(text(),'Dividend Distributions')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h2[contains(text(),'Fund Management')]//parent::div//div[@class=\"view-content\"]/div"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//h4[@class=\"views-field views-field-field-last-name author-name\"]/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    ".//div[@class=\"views-field views-field-field-industry-experience industry-experience\"]/text()"
                ],
                "cleanup_functions": [
                    "obj['fund_manager_years_of_experience_in_industry'].replace('Industry Experience: ', \"\")"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}