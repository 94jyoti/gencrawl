{
    "website": "https://partnerselectfunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "partnerselectfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://partnerselectfunds.com/alternative-strategies-fund/",
            "https://partnerselectfunds.com/equity-fund/",
            "https://partnerselectfunds.com/high-income-alternatives-fund/",
            "https://partnerselectfunds.com/international-fund/",
            "https://partnerselectfunds.com/funds/oldfield-international-value-fund/",
            "https://partnerselectfunds.com/sbh-focused-small-value-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[contains(text(),'Fund')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//th[contains(text(),'Performance')]/ancestor::thead/following-sibling::tbody/tr/td[contains(text(),'Fund') and not(contains(text(),'Index'))]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//th[contains(text(),'Expense Ratios')]/following-sibling::th/text()[not(contains(.,'Ratio'))]",
                    "//td[contains(text(),'Symbol')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'Cusip')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()[contains(.,'Class')]"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('(')[-1].split('Class')[0]"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//td[contains(text(),'12B-1 Fees')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Gross Expense Ratio')]/ancestor::thead/following-sibling::tbody/tr/td[2]/text()",
                    "//td[contains(text(),'Gross Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//th[contains(text(),'Gross Expense Ratio')]/ancestor::thead/following-sibling::tbody/tr/td[3]/text()",
                    "//td[contains(text(),'Net Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Investment')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['minimum_initial_investment'].split('(')[0].split('retirement')[0].split('Retirement')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//td[contains(text(),'Index')]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class = 'manager_list']/div/figure/following-sibling::div/h4"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}