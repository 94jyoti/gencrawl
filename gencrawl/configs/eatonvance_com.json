{
  "website": "funds.eatonvance.com",
  "spider": "financial_detail",
  "domain": "financial",
  "crawl_method": "SCRAPY_GET",
  "allowed_domains": [
    "funds.eatonvance.com"
  ],
  "parsing_type": "xpath",
  "country": "US",
  "language": "ENGLISH",
  "listing": {
    "crawl_method": "SCRAPY_GET",
    "parsing_type": "xpath",
    "start_urls": [
      "https://funds.eatonvance.com/"
    ]
  },
  "detail": {
    
    "crawl_method": "SCRAPY_GET",
    "parsing_type": "xpath",
    "custom_settings":{
    "RETRY_HTTP_CODES":[400, 500, 502, 503, 504, 520, 522, 524, 408, 403, 429,404]
    },
    "pagination":{
     "paths":["//ul[@class='shares']/li[not(@class='active')]/a/@href"],
     "return_type":"list"
    },
    "ext_codes": {
      "instrument_name": {
        "paths": [
          "//div[@class='fund_title']//h1/text()"]
      } ,
      "share_class": {
        "paths": [
          "//div[@class='shares']/a//span//text()","//div[@class='fund_title']//h1//span//text()"
        ],
        "cleanup_functions": ["obj.get('share_class').replace(')','').replace('(','')"]
      },
      "nasdaq_ticker": {
        "paths": [
          "//span[@class='symbol']//text()"
        ]
      },
      "cusip":{
        "paths":["//td[contains(text(),'CUSIP')]//following-sibling::td//text()"]
      },
      "temp_assets": {
        "paths": [
          "//td[contains(text(),'Total Net Assets')]//following-sibling::td//text()"],
          "return_type":"list"
      },
      "portfolio_assets_date":{
        "paths":["//span[contains(text(),'Fund Facts')]//following-sibling::span//text()"]
      },
      "total_expense_gross":{
        "paths":["//span[contains(text(),'(Gross)')]//parent::td//following-sibling::td/text()","//td[contains(text(),'Expense')]//following-sibling::td//text()"]
      },
      "total_expense_net":{
        "paths":["//span[contains(text(),'(Net)')]//parent::td//following-sibling::td/text()"]
      },
      "share_inception_date":{
       "paths":["//td[contains(text(),'Class')]//following-sibling::td//text()","//td[contains(text(),'Performance')]//following-sibling::td//text()"]
      },
      "effective_duration":{
        "paths":[
          "//td[contains(text(),'Effective Duration')]//following-sibling::td//text()"
        ]
       },
       "duration":{
        "paths":["//td[text()=('Duration')]//following-sibling::td//text()","//td[text()=('Average Duration')]//following-sibling::td//text()"]
       },
       "duration_as_of_date":{
        "paths":["//span[contains(text(),'Portfolio Statistics')]//following-sibling::span//text()"]
       },
      "average_weighted_maturity":{
        "paths":[
          "//td[contains(text(),'Average Maturity')]//following-sibling::td//text()"
        ]
       },
       "average_effective_maturity":{
        "paths":["//td[contains(text(),'Average Effective Maturity')]//following-sibling::td//text()"]
       },
       "average_effective_maturity_as_of_date":{
        "paths":["//span[contains(text(),'Portfolio Statistics')]//following-sibling::span//text()"]
       },
       
       "effective_duration_date":{
        "paths":["//span[contains(text(),'Portfolio Statistics')]//following-sibling::span//text()"]
       },
       "average_weighted_maturity_as_of_date":{
        "paths":["//span[contains(text(),'Portfolio Statistics')]//following-sibling::span//text()"]
       },
       "minimum_initial_investment":{
        "paths":["//td[contains(text(),'Minimum Investment')]//following-sibling::td//text()"]
       },
       "temp_fund_managers":{
        "paths":["//table[@name='PORTFOLIOMANAGEMENT']//tr"],   
        "return_type":"selector",
        "child_return_strategy": "multiple_objects"
       },
       "fund_manager":{
        "paths":[".//td[position()=1]//text()"],
        "selector":"temp_fund_managers"
       },
       "fund_manager_years_of_experience_with_fund":{
        "paths":[".//td[last()]//text()"],
        "selector":"temp_fund_managers"
       
       },
       "temp_capital_gain":{
        "paths":["//table[@name='CAPITALGAINHISTORY']//tbody//tr//td"],
        "return_type":"list"
       },
       "distribution_frequency":{
        "paths":["//td[contains(text(),'Distribution Frequency')]//following-sibling::td//text()"]
       },
       "sec_yield_30_day":{
        "paths":["//td[contains(text(),'Subsidized SEC')]//following-sibling::td//text()","//td[contains(text(),'SEC 30-day')]//following-sibling::td//text()"]
       },
       "sec_yield_date_30_day":{
        "paths":["//span[contains(text(),'Yield Information')]//following-sibling::span//text()"]
       },
       "sec_yield_without_waivers_30_day":{
       "paths":["//td[contains(text(),'Unsubsidized SEC')]//following-sibling::td//text()"]
       },
       "sec_yield_without_waivers_date_30_day":{
        "paths":["//span[contains(text(),'Yield Information')]//following-sibling::span//text()"
        ]
       },
       "temp_benchmark":{
        "paths":["//span[contains(text(),'Calendar Year Returns')]//ancestor::table//tbody//tr[position()>1]//span[contains(text(),'Index')]//parent::td"],
        "return_type":"list"
       }
    }
      }
    }
