{
    "website": "https://www.driehaus.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "driehaus.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_driehaus_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.driehaus.com/funds/driehaus-emerging-markets-growth-fund",
            "https://www.driehaus.com/funds/driehaus-small-cap-growth-fund",
            "https://www.driehaus.com/funds/driehaus-micro-cap-growth-fund",
            "https://www.driehaus.com/funds/driehaus-smallmid-cap-growth-fund",
            "https://www.driehaus.com/funds/driehaus-international-small-cap-growth-fund",
            "https://www.driehaus.com/funds/driehaus-emerging-markets-small-cap-growth-fund",
            "https://www.driehaus.com/funds/driehaus-emerging-markets-opportunities-fund",
            "https://www.driehaus.com/funds/driehaus-event-driven-fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h4[contains(text(),'Funds')]/following-sibling::h2/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//dt[contains(text(),'Ticker')]/following-sibling::dd"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//dt[text()='Class']/following-sibling::dd[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_net_assets": {
                "paths": [
                    "//dt[contains(text(),'Fund Assets')]/following-sibling::dd[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//dt[contains(text(),'Fund Assets')]/text()"
                ],
                "cleanup_functions": [
                    "[o.split('as of')[-1] for o in obj['total_net_assets_date']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//dt[contains(text(),'Gross Expense Ratio')]/following-sibling::dd[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//dt[contains(text(),'Net Expense Ratio')]/following-sibling::dd[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//dt[contains(text(),'Inception Date')]/following-sibling::dd[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//dt[contains(text(),'Minimum Initial Investment')]/following-sibling::dd[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//dt[contains(text(),'Minimum Subsequent Investment')]/following-sibling::dd[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//a[contains(text(),'Dividends & Distributions')]/parent::dt/following-sibling::dd[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//a[contains(text(),'Dividends & Distributions')]/parent::dt/following-sibling::dd[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//h3[contains(text(),'Month-End Performance')]/parent::div/div/table/tbody/tr/td[contains(text(),'Index')]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h2[contains(text(),'Portfolio Managers')]/parent::div/div/a/p[1]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}