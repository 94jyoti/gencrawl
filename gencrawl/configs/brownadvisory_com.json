{
    "website": "https://www.brownadvisory.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "brownadvisory.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.brownadvisory.com/mf/funds/sustainable-bond-fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[@class='page-header__title']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//select[contains(@class,'share-class')]//option"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//th[contains(text(),'Share Class')]/parent::tr/parent::thead/parent::table/tbody/tr[1]/td[1]/text()"
                ],
                "cleanup_functions": [
                    "[o.split(',')[0].split('(')[-1].replace(\")\",\" \") for o in obj['nasdaq_ticker']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "total_expense_gross": {
                "paths": [
                    "//strong[contains(text(),'Expense Ratio')]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//strong[contains(text(),'Expense Ratio')]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//th[contains(text(),'Share Class')]/parent::tr/parent::thead/parent::table/tbody/tr[1]/td[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "effective_duration": {
                "paths": [
                    "//td[contains(text(),'Effective Duration')]/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration_date": {
                "paths": [
                    "//h4[contains(text(),'Characteristics')]/parent::div/div/div/header/p/text()"
                ],
                "cleanup_functions": [
                    "obj['effective_duration_date'].replace('As of ', '').replace(')', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//td[contains(text(),'30-Day SEC Yield Subsidized (%)')]/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "//h4[contains(text(),'Characteristics')]/parent::div/div/div/header/p/text()"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_date_30_day'].replace('(As of ', '').replace(') ', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_30_day": {
                "paths": [
                    "//td[contains(text(),'30-Day SEC Yield Unsubsidized (%)')]/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_date_30_day": {
                "paths": [
                    "//h4[contains(text(),'Characteristics')]/parent::div/div/div/header/p/text()"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_without_waivers_date_30_day'].replace('(As of ', '').replace(') ', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h2[@class='advisor__name']/a/span"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}