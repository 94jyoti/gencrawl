{
    "website": "http://www.eipfunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "eipfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "http://www.eipfunds.com/products/mutual-funds/"
        ],
        "ext_codes": {
            "temp_class_blocks": {
                "paths": [
                    "//span[contains(text(),'Ticker')]/parent::td/following-sibling::td/span"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//span[contains(text(),'CUSIP')]/parent::td/following-sibling::td/span"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//span[contains(text(),'Total Assets')]/parent::td/following-sibling::td/span"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "(//span[contains(text(),'12b-1 fee')]/parent::td/following-sibling::td/span)"
                ],
                "cleanup_functions": [
                    "['None', obj.get('fees_total_12b_1')[0]]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "initial_sales_charge": {
                "paths": [
                    "//span[contains(text(),'Maximum front-end sales load')]/parent::td/following-sibling::td/span"
