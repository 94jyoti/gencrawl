                "return_type": "list-map",
                "selector": "root"
            },
            "deferred_sales_charge": {
                "paths": [
                    "//span[contains(text(),'Maximum deferred-end sales load')]/parent::td/following-sibling::td/span"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//span[contains(text(),'Redemption fee')]/parent::td/following-sibling::td/span"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//span[contains(text(),'Gross Expense Ratio')]/parent::td/following-sibling::td/span"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//span[contains(text(),'Net Expense Ratio')]/parent::td/following-sibling::td/span"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//strong[contains(text(),'Inception')]/parent::span"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h2[contains(text(),'Objective')]/following-sibling::p"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h3[contains(text(),'Investment Strategy')]/following-sibling::p"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//span[contains(text(),'Minimum Investment')]/parent::td/following-sibling::td/span"
                ],
                "return_type": "list-map",
                "selector": "root"
