{
    "website": "https://www.dpimc.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "dpimc.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.virtus.com/products/duff-phelps-global-infrastructure#shareclass.I/period.quarterly"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class = 'product-name']/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//span[@class = 'symbol']"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//span[@class = 'cusip']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "(//select [@id = 'share-class']/option)/text()"
                ],
                "return_type": "list-map",
                "selector": "temp_class_blocks"
            },
            "total_net_assets": {
                "paths": [
                    "//span[@data-format = 'two_decimal' and @class = 'assets-by-class']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//div [contains(text(),'Total Assets by Class')]/following-sibling::small/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//span[@data-format='date'][@class='inception']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//span[@class='Yield_SEC']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "//span[@class='AsOfDate_DstrbYld']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//dt[@class='share-index1-name share-name']/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_capital_gains": {
                "paths": [
                    "(//div[@class = 'mgr-info']/h4)"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            }
        }
    }
}