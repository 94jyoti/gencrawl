{
    "website": "https://www.bairdassetmanagement.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "bairdassetmanagement.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_bairdassetmanagement_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.bairdassetmanagement.com/baird-funds/ultra-short-term-bond-fund",
            "https://www.bairdassetmanagement.com/baird-funds/short-term-bond-fund",
            "https://www.bairdassetmanagement.com/baird-funds/intermediate-bond-fund",
            "https://www.bairdassetmanagement.com/baird-funds/bond-aggregate-fund",
            "https://www.bairdassetmanagement.com/baird-funds/core-plus-bond-fund",
            "https://www.bairdassetmanagement.com/baird-funds/short-term-municipal-bond-fund",
            "https://www.bairdassetmanagement.com/baird-funds/strategic-municipal-bond-fund",
            "https://www.bairdassetmanagement.com/baird-funds/quality-intermediate-municipal-bond-fund",
            "https://www.bairdassetmanagement.com/baird-funds/core-intermediate-municipal-bond-fund",
            "https://www.bairdassetmanagement.com/baird-funds/municipal-bond-fund",
            "https://www.bairdassetmanagement.com/baird-funds/equity-small-cap-value-fund",
            "https://www.bairdassetmanagement.com/baird-funds/equity-small-mid-cap-value-fund",
            "https://www.bairdassetmanagement.com/baird-funds/equity-small-mid-cap-growth-fund",
            "https://www.bairdassetmanagement.com/baird-funds/equity-mid-cap-fund",
            "https://www.bairdassetmanagement.com/baird-funds/chautauqua-global-growth-fund",
            "https://www.bairdassetmanagement.com/baird-funds/chautauqua-international-growth-fund"
            
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[contains(text(),'Fund')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//div[@id='Overview']//select[@class='ddlShareClass']/option"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "asset_class": {
                "paths": [
                    "//td[contains(text(),'Style')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Total Fund AUM')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//label[contains(text(),'Data')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[@class = 'gross-net']/span[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[@class = 'gross-net']/span[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//td[@class='fundName'][contains(text(),'Index')]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//img[@class='fluid']"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./@alt"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
            
        }
    }
}