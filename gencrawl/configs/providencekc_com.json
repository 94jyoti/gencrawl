{
    "website": "https://www.providencekc.com/",
    "domain": "hospital",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "providencekc.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.providencekc.com/find-a-provider/michael-a-adamovich-aprn/",
            "https://www.providencekc.com/find-a-provider/nicholas-s-aberle-md/"
        ],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "speciality": {
                "paths": [
                    "//strong[contains(text(),'Specialty')]/following-sibling::div/span/a/text()| //strong[contains(text(),'Specialty')]/following-sibling::span/a/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "address_raw": {
                "paths": [
                    "//strong[contains(text(), 'Locations')]/following-sibling::ul/li/address"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "practice_name": {
                "paths": [
                    "//strong[contains(text(), 'Locations')]/following-sibling::ul/li/strong/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            }
        }
    }
}