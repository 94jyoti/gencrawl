{
    "website": "https://www.barrettasset.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "barrettasset.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.barrettasset.com/barrettopportunity/",
            "https://www.barrettasset.com/barrettmutual/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//strong[contains(text(),\"FUND\")]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//span[contains(text(),\"Symbol\")]/text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split(':')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//strong[contains(text(),\"Investment Objective \")]/following::span[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//strong[contains(text(),\"Investment Strategy \")]/following::span[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sub_advisor": {
                "paths": [
                    "//strong[contains(text(),'Investment Management ')]/following::span[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['sub_advisor'].split('by ')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}