{
    "website": "https://northsquareinvest.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "northsquareinvest.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://northsquareinvest.com/strategies/all-cap-value-fund/",
            "https://northsquareinvest.com/strategies/mckee-bond-fund/",
            "https://northsquareinvest.com/strategies/small-cap-growth/",
            "https://northsquareinvest.com/strategies/altrinsic-international-equity-fund/",
            "https://northsquareinvest.com/strategies/dynamic-small-cap/",
            "https://northsquareinvest.com/strategies/multi-strategy/",
            "https://northsquareinvest.com/strategies/disciplined-growth/",
            "https://northsquareinvest.com/strategies/dividend-growth-fund/",
            "https://northsquareinvest.com/strategies/strategic-income-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//th[contains(text(),'Class')]/parent::tr/parent::thead/following-sibling::tbody/tr/td[1]",
                    "//strong[contains(text(),'Class')]/parent::td/parent::tr/following-sibling::tr/td[1][text()!='']"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//th[contains(text(),'Ticker')]/parent::tr/parent::thead/following-sibling::tbody/tr/td[2]/text()",
                    "//strong[contains(text(),'Ticker')]/parent::td/parent::tr/following-sibling::tr/td[2]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_management_style": {
                "paths": [
                    "//div[contains(text(),'Style')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//div[contains(text(),'Current Fund AUM')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Gross Expense')]/parent::tr/parent::thead/following-sibling::tbody/tr/td[4]/text()",
                    "//strong[contains(text(),'Gross Expense')]/parent::td/parent::tr/following-sibling::tr/td[4]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//th[contains(text(),'Net Expense')]/parent::tr/parent::thead/following-sibling::tbody/tr/td[3]/text()",
                    "//strong[contains(text(),'Net Expense')]/parent::td/parent::tr/following-sibling::tr/td[3]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//div[contains(text(),'Inception Date')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//div[contains(text(),'Turnover')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//div[contains(text(),'Benchmark')]/following-sibling::div/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//span[contains(text(),'Portfolio Managers')]/parent::h3/following-sibling::div//div/div/div/following-sibling::div/div[1]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}