        "parsing_type": "xpath",
        "start_urls": [
            "https://www.johcm.com//us/our-funds/fund-details-AIT-GBX/johcm-global-income-builder-fund-jobix#overview",
            "https://www.johcm.com//us/our-funds/fund-details-AIT-CIF/johcm-credit-income-fund#overview",
            "https://www.johcm.com//us/our-funds/fund-details-AIT-EMO/johcm-emerging-markets-opportunities-fund-joeix#overview",
            "https://www.johcm.com//us/our-funds/fund-details-AIT-EMSC/johcm-emerging-markets-small-mid-cap-equity-fund-jomex#overview",
            "https://www.johcm.com//us/our-funds/fund-details-AIT-INO/johcm-international-opportunities-fund-jopsx#overview",
            "https://www.johcm.com//us/our-funds/fund-details-AIT-INS/johcm-international-select-fund-johix#overview",
            "https://www.johcm.com//us/our-funds/fund-details-AIT-GLB/global-select-fund-jogex#overview",
            "https://www.johcm.com//us/our-funds/fund-details-AITRGEI/regnan-global-equity-impact-solutions-regix#overview"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h3/span/text()"
                ],
                "cleanup_functions": [
                    "obj['field_name'].split('(')[0] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "((//table[@class=\"fundcodes-table\"])[1]//tbody//td[2])[position()>1]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(.,'Gross Expense Ratio*')]//td[contains(.,'X')]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "(//td//b[contains(.,'CUSIP')])[1]//ancestor::tr[1]//following-sibling::tr//td[4]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//th[contains(.,'Fund size')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h3[contains(text(),'Fund details')]//following-sibling::h5//text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace(\"as at\",\"\").strip()",
                    "obj['field_name'].split('Data')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "(//td//b[contains(.,'Gross Expense')])[1]//ancestor::tr[1]//following-sibling::tr//td[4]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "(//td//b[contains(.,'Net Expense')])[1]//ancestor::tr[1]//following-sibling::tr//td[3]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//table[@class='shareclass-table ']//tbody//tr//td[2]//text()"
                ],
                "cleanup_functions": [
                    "[o.split(') ')[-1] for o in obj['share_inception_date']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h5[contains(text(),'Investment Objective')]//following-sibling::p//text()",
                    "//h5[contains(text(),'Investment objective')]//following-sibling::p//text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h5[contains(text(),'Investment Strategy')]/following-sibling::ul//li//text()",
                    "(//h5[contains(text(),'Investment Strategy')]/following-sibling::text())[1]",
                    "//h5[contains(text(),'Investment Strategy')]/following-sibling::p[not(contains(.,'JOHCM') or contains(.,'%'))]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//table[@class='mininvest-table']//td[contains(.,'$')]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "sub_advisor": {
                "paths": [
                    "//th[contains(text(),'Investment')]//following-sibling::td//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//th[contains(.,'Benchmark')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//ul[@class='team details-people margn0 listnone']//p[contains(.,'Fund Manager')]/preceding-sibling::h6"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}