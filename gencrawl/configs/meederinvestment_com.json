{
    "website": "http://www.meederinvestment.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "meederinvestment.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Balanced-Fund/Share-Class/Retail",
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Conservative-Allocation-Fund/Share-Class/Retail",
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Dynamic-Allocation-Fund/Share-Class/Retail",
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Global-Allocation-Fund/Share-Class/Retail",
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Institutional-Prime-Money-Market-Fund/Share-Class/Institutional",
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Moderate-Allocation-Fund/Share-Class/Retail",
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Muirfield-Fund/Share-Class/Retail",
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Prime-Money-Market-Fund/Share-Class/Retail",
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Quantex-Fund/Share-Class/Retail",
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Spectrum-Fund/Share-Class/Retail",
            "https://www.meederinvestment.com/Products-and-Services/Mutual-Funds/Fact-Sheets/Meeder-Tactical-Income-Fund/Share-Class/Retail"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class='fact-sheet-fund-name']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//div[contains(text(),'Symbol')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//div[contains(text(),'CUSIP')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//div[contains(text(),'Total Net Assets')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//div[contains(text(),'Inception')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//div[contains(text(),'Investment Objective')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//div[contains(text(),'Investment Strategy')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//div[contains(text(),'Minimum Initial Investment')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//div[contains(text(),'Turnover Rate')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[contains(text(),'Portfolio Management')]/following-sibling::div/div/div/div/div[1]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        },
        "pagination": {
            "pagination": {
                "paths": [
                    "//div[contains(text(),'Share Class:')]/span/select/option/@value"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}