{
  "website": "russellinvestments.com",
  "spider": "financial_detail",
  "domain": "financial",
  "crawl_method": "SCRAPY_GET",
  "allowed_domains": [
    "russellinvestments.com"
  ],
  "parsing_type": "xpath",
  "country": "US",
  "language": "ENGLISH",
  "detail": {
    "crawl_method": "SCRAPY_GET",
    "parsing_type": "jpath",
    "ext_codes": {
      "temp_info_json_data": {
        "paths": [
          "//*[@id='FundV2DetailContainer']//@value"
        ],
        "return_type":"selector-json",
        "child_return_strategy": "single_item",
        "parsing_type": "xpath",
        "cleanup_functions": [
          "obj.get('temp_info_json_data')[0]['fund']",
          "[json.loads(obj.get('temp_info_json_data'))]"
        ]
      },
      "fund_name": {
        "paths": [
          "['Fund']['FundName']"
        ],
        "selector": "temp_info_json_data"
      },
      "share_class": {
        "paths": [
          "['ShareClass']"
        ],
        "selector": "temp_info_json_data"
      }

    }
  }
}
