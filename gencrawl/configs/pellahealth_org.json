{
    "website": "https://www.pellahealth.org/",
    "domain": "hospital",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "pellahealth.org"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.pellahealth.org/doctors/doctor-profile/ryan-vincent/"
        ],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "//span[@class='DrName']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "designation": {
                "paths": [
                    "//span[@class='DrTitles']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "speciality": {
                "paths": [
                    "//div[@id='Specialty']/p/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "address_raw": {
                "paths": [
                    "//div[@class='Locations']"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "phone": {
                "paths": [
                    "//a[@class='Tel']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}