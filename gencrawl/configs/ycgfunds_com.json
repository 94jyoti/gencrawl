{
    "website": "https://ycgfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "ycgfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://ycgfunds.com/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//h5[contains(text(),\"Fund Performance\")]//following::td[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split('Fund ')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//h5[contains(text(),\"FUND CHARACTERISTICS\")]//following::td[contains(text(),\"Assets\")]//following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h5[contains(text(),\"FUND CHARACTERISTICS\")]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('OF' )[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//*[contains(text(),\" Our Investment Strategy\")]//following::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//h5[contains(text(),\"FUND CHARACTERISTICS\")]//following::td[contains(text(),\"Turnover Ratio\")]//following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h5[contains(text(),\"FUND CHARACTERISTICS\")]/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].split('OF')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//h5[contains(text(),\"Fund Performance\")]//following::td[contains(text(),\"Index\")]"
                ],
                "cleanup_functions": [
                    "obj['benchmarks'].replace('3', '')"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}