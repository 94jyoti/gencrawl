{
    "website": "https://www.mmhealth.org/",
    "domain": "hospital",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "mmhealth.org"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.mmhealth.org/doctor/cindy-allen-fnp-c/",
            "https://www.mmhealth.org/doctor/khaled-abdeljawad-md/",
            "https://www.mmhealth.org/doctor/marcy-baumer-pa-c/",
            "https://www.mmhealth.org/doctor/katie-bailey-np/",
            "https://www.mmhealth.org/doctor/erica-bayoneto-dds/",
            "https://www.mmhealth.org/doctor/krista-bedenkop-fnp-bc/",
            "https://www.mmhealth.org/doctor/michelle-biltz-md/",
            "https://www.mmhealth.org/doctor/loretta-bischoff-fnp-c/"
        ],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "//*[contains(@class, 'dr-name')]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "speciality": {
                "paths": [
                    "(//*[contains(text() , 'Specialt' )]/following-sibling::*)[1]"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "address_raw": {
                "paths": [
                    "//div[@id='locations']//div[contains(@class,'location info')]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "phone": {
                "paths": [
                    "//div[@id='locations']//div[@class='contact-number info']"
                ],
                "return_type": "list-map",
                "selector": "root"
            }
        }
    },
    "decision_tags": {
        "practice_in_address": "1"
    }
}