{
    "website": "https://www.dunham.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "dunham.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.dunham.com/Investor/FundInfo/CorpGov",
            "https://www.dunham.com/Investor/FundInfo/FloatingRate",
            "https://www.dunham.com/Investor/FundInfo/HighYield",
            "https://www.dunham.com/Investor/FundInfo/InternationalBond",
            "https://www.dunham.com/Investor/FundInfo/LargeCapValue",
            "https://www.dunham.com/Investor/FundInfo/SmallCapValue",
            "https://www.dunham.com/Investor/FundInfo/FocusedGrowth",
            "https://www.dunham.com/Investor/FundInfo/SmallCapGrowth",
            "https://www.dunham.com/Investor/FundInfo/EmergingMarket",
            "https://www.dunham.com/Investor/FundInfo/InternationalStock",
            "https://www.dunham.com/Investor/FundInfo/DynamicMacro",
            "https://www.dunham.com/Investor/FundInfo/LongShortCredit",
            "https://www.dunham.com/Investor/FundInfo/MonthlyDistribution",
            "https://www.dunham.com/Investor/FundInfo/RealEstateStock"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//body[@id='fundInfo']/section/div//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'Cusip')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//body[@id='fundInfo']/section//h3/text()[contains(.,'Class')]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Fund Inception')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h4[contains(text(),'Fund Objective')]/parent::div/p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//td[contains(text(),'Capital Gains Paid')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['distribution_frequency'].replace('*',' ')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//td[contains(text(),'Dividend Frequency')]/following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['dividend_frequency'].replace('*',' ')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//th[contains(text(),'30 Day SEC Yield')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "(//th[contains(text(),'As of')]/parent::tr/th/span)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//h4[contains(text(),'Sub-Advisor')]/parent::div/p/span/text()"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}