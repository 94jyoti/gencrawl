{
    "website": "https://www.griffincapital.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "griffincapital.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_griffincapital_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.griffincapital.com/griffin-institutional-access-real-estate-fund/about/overview-and-strategy/",
            "https://www.griffincapital.com/griffin-institutional-access-credit-fund/about/overview-and-strategy/"
            
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//a[contains(text(),'About')]/preceding-sibling::a[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h2[contains(text(),'Objective') or contains(text(),'OBJECTIVE')]/following::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h2[contains(text(),'Strategy') or contains(text(),'STRATEGY')]/following::p[not(count(preceding::center[1])=1)]"
                ],
                "return_type": "join",
                "selector": "root"
            }
        }
    }
}