{
    "website": "https://www.vericimetry.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "vericimetry.com","hosting.umb.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.vericimetry.com/vysvx/performance.php"
        ],
        "ext_codes": {
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/parent::tr/following-sibling::tr[1]/td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/parent::tr/following-sibling::tr[1]/td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "(//td[contains(text(),'Annual Expenses')]/parent::tr/following-sibling::tr[1]/td/br/following::text())[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "(//td[contains(text(),'Annual Expenses')]/parent::tr/following-sibling::tr[1]/td/br/following::text())[3]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/parent::tr/following-sibling::tr[1]/td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}