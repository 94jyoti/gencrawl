{
    "website": "http://www.cullenfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "cullenfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "http://www.cullenfunds.com/mutual-funds/high-dividend/overview",
            "http://www.cullenfunds.com/mutual-funds/international-high-dividend-ord/overview",
            "http://www.cullenfunds.com/mutual-funds/emerging-markets-high-dividend/overview",
            "http://www.cullenfunds.com/mutual-funds/enhanced-equity-income/overview",
            "http://www.cullenfunds.com/mutual-funds/value-equity/overview",
            "http://www.cullenfunds.com/mutual-funds/small-cap-value-equity/overview"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class = 'sub-head']/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//*[contains(text(),'Ticker:')]/following-sibling::td"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//*[contains(text(),'CUSIP:')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//th[contains(text(),'Class')]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//*[contains(text(),'Redemption Fee*:')]/following-sibling::td/audit/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//*[contains(text(),'Gross Expense Ratio:')]/following-sibling::td/audit/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//*[contains(text(),'Net Expense Ratio:')]/following-sibling::td/audit/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "(//*[text()='Objective']//following-sibling::p)[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//*[contains(text(),'Minimum Investment:')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            }
        }
    }
}