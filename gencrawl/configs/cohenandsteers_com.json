{
    "website": "https://www.cohenandsteers.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "cohenandsteers.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.cohenandsteers.com/funds/details/alternative-income-fund",
            "https://www.cohenandsteers.com/funds/details/global-infrastructure-fund",
            "https://www.cohenandsteers.com/funds/details/global-realty-shares",
            "https://www.cohenandsteers.com/funds/details/institutional-realty-shares",
            "https://www.cohenandsteers.com/funds/details/international-realty-fund",
            "https://www.cohenandsteers.com/funds/details/cohen-steers-low-duration-preferred-and-income-fund",
            "https://www.cohenandsteers.com/funds/details/cohen-steers-mlp-and-energy-opportunity-fund",
            "https://www.cohenandsteers.com/funds/details/preferred-securities-and-income-fund",
            "https://www.cohenandsteers.com/funds/details/real-assets-fund",
            "https://www.cohenandsteers.com/funds/details/cohen-steers-real-estate-securities-fund",
            "https://www.cohenandsteers.com/funds/details/realty-shares"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//header[@class='page-title']/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "(//div[@class=\"sidebar-block\"])[1]//h6[contains(text(),'Share Class/Symbol')]/parent::header/following-sibling::li//span[2]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "(//div[@class=\"sidebar-block\"])[1]//h6[contains(text(),'Share Class/Symbol')]/parent::header/following-sibling::li//span[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "(//div[@class=\"sidebar-block\"])[1]//h6[contains(text(),'Total Net Assets')]/parent::header/following-sibling::h3/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "(//div[@class=\"sidebar-block\"])[1]//h6[contains(text(),'Total Net Assets')]/parent::header/following-sibling::p/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace('at', '').replace('(', '').replace(')', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "(//div[@class=\"sidebar-block\"])[1]//h6[contains(text(),'Inception Date')]/parent::header/following-sibling::h3/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//div[@id='fund-overview']/h3/p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield": {
                "paths": [
                    "(//div[@class=\"sidebar-block\"])[1]//h6[contains(text(),'SEC Yield')]/parent::header/following-sibling::h3/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//li[@class='team-member']/a/div/h6"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}