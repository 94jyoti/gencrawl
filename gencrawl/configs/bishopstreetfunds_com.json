{
    "website": "http://www.bishopstreetfunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "bishopstreetfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "http://www.bishopstreetfunds.com/fi-hgrdif-ins.html",
            "http://www.bishopstreetfunds.com/fi-hmunbond-ins.html",
            "http://www.bishopstreetfunds.com/fi-hmunbond-ret.html"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h2/text()"
                ],
                "cleanup_functions": [
                    "obj['instrument_name'].split('-')[0] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//*[contains(text(),\"Fund Details\")]//following::*[contains(text(),\"Ticker Symbols\")]//following-sibling::*"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split('- ')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//h2/text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('- ')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets": {
                "paths": [
                    "//*[contains(text(),'Total Net Assets')]/following::td[3]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//*[@class=\"monthlyDate\"]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//*[contains(text(),\"Fund Details\")]//following::*[contains(text(),\"Maximum Sales Charges\")]//following-sibling::td/text()"
                ],
                "cleanup_functions": [
                    "obj['maximum_sales_charge_full_load'].split('- ')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//*[contains(text(),\"Fund Details\")]//following::*[contains(text(),\"Inception Date\")]//following-sibling::td[contains(.,\"Class\")]"
                ],
                "cleanup_functions": [
                    "obj['share_inception_date'].split('Shares - ')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//*[contains(text(),\"Investment Objective\")]//following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//*[contains(text(),\"Investment Strategy\")]//following-sibling::*/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "duration": {
                "paths": [
                    "(//*[contains(text(),\"Fund Details\")]//following::*[contains(text(),\"Duration\")]//following::*[1])[3]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "duration_as_of_date": {
                "paths": [
                    ".//*[contains(text(),'Duration')]//*[@class='quarterlyDate']//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity": {
                "paths": [
                    "(//*[contains(text(),\"Fund Details\")]//following::*[contains(text(),\"Average Weighted Maturity\")]//following::*[1])[3]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity_as_of_date": {
                "paths": [
                    ".//*[contains(text(),'Average Weighted Maturity')]//*[@class='quarterlyDate']//text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}