{
    "website": "https://tfafunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "tfafunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_tfafunds_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://tfafunds.com/conservative-fund/",
            "https://tfafunds.com/moderate-fund/",
            "https://tfafunds.com/growth-fund/",
            "https://tfafunds.com/multidimensional-fund/",
            "https://tfafunds.com/quantitative-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "(//div[@class = 'wpb_wrapper']/h2)[1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//span[contains(text(),'Share')]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//h3[contains(text(),'Class')]/text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'][0].split('|')",
                    "[nt.split(':')[-1] for nt in obj['nasdaq_ticker']]",
                    "[o for o in obj['nasdaq_ticker'] if 'N/A' not in o]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_consultant": {
                "paths": [
                    "//h3[contains(text(),'Investment Advisor')]/following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//th[contains(text(),'Distribution and Service (12b-1) Fees')]/following-sibling::th/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//th[contains(text(),'Management Fees')]/following-sibling::th/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Gross Annual Fund Operating Expenses')]/following-sibling::th/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//th[contains(text(),'Net Annual Fund Operating Expenses')]/following-sibling::th/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h3[contains(text(),'Fund Objective')]/following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h3[contains(text(),'Fund Strategy')]/following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "(//th[contains(text(),'Minimum Initial Investment')]/following-sibling::th)/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "(//th[contains(text(),'Minimum Subsequent Investment')]/following-sibling::th)/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "sub_advisor": {
                "paths": [
                    "//h3[contains(text(),'Sub-Advisors')]/following-sibling::p/text()"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}