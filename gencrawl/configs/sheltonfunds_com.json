{
    "website": "https://sheltonfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "sheltonfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://sheltonfunds.com/domestic-stock-funds/shelton-core-value-fund/",
            "https://sheltonfunds.com/domestic-stock-funds/shelton-nasdaq-100/",
            "https://sheltonfunds.com/domestic-stock-funds/sp-500/",
            "https://sheltonfunds.com/domestic-stock-funds/sp-midcap/",
            "https://sheltonfunds.com/domestic-stock-funds/sp-smallcap/",
            "https://sheltonfunds.com/domestic-stock-funds/shelton-green-alpha-fund-nextx/",
            "https://sheltonfunds.com/domestic-stock-funds/shelton-bdc-income/",
            "https://sheltonfunds.com/international-stock-funds/international-select-equity/",
            "https://sheltonfunds.com/international-stock-funds/shelton-emerging-markets-fund/",
            "https://sheltonfunds.com/bond-funds/shelton-tactical-credit/",
            "https://sheltonfunds.com/bond-funds/green-california-tax-free/",
            "https://sheltonfunds.com/bond-funds/us-government/",
            "https://sheltonfunds.com/money-market/us-treasury-trust/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/span[contains(text(),'(')]/text()"
                ],
                "cleanup_functions": [
                    "obj['instrument_name'].split('(')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//h1/span[contains(text(),'(')]/text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split('(')[-1].split(')')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h2[contains(text(),'Objective')]/following-sibling::p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h2[contains(text(),'Strategy Highlights')]/following-sibling::ul/li/span/text()",
                    "//h2[contains(text(),'Strategy Highlights')]/following-sibling::ul/li/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class = 'awsm-personal-info']/h3"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}