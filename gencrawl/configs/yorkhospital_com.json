{
    "website": "https://www.yorkhospital.com/",
    "domain": "hospital",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "yorkhospital.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "//header/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "speciality": {
                "paths": [
                    "//div[@class='providerSpecialty']/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "address_raw": {
                "paths": [
                    "//h3[contains(text(),'Location')]/following-sibling::p[not(contains(@class, 'Hours'))]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "practice_name": {
                "paths": [
                    "//h3[contains(text(),'Location')]/following-sibling::h5/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "phone": {
                "paths": [
                    "//div[@class='providerPhone']/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "fax": {
                "paths": [
                    "//div[@class='providerFax']/text()"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}