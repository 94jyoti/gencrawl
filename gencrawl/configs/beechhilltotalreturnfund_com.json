{
    "website": "http://www.beechhilltotalreturnfund.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "beechhilltotalreturnfund.com","http://www.geminifund.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "http://www.beechhilltotalreturnfund.com/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h2[contains(text(),'At a Glance')]/following-sibling::p[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['instrument_name'].split('is')[0] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//span[contains(text(),'Ticker')]//parent::td//parent::tr//following::tr/td[1]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//h2[contains(text(),'At a Glance')]/following-sibling::p[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('in')[-1] ",
                    "obj['share_class'].replace('shares.','')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Total fund assets')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//td[contains(text(),'Total fund assets')]/following-sibling::td/span/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Fund inception date')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h2[contains(text(),'Manager')]//following::p[1]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//strong//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    ".//strong//following::text()[1]"
                ],
                "cleanup_functions": [
                    "re.findall(r'\\d*\\.?\\d+',obj['fund_manager_years_of_experience_in_industry'])[0]"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_with_fund": {
                "paths": [
                    ".//strong//following::text()[1]"
                ],
                "cleanup_functions": [
                    "re.findall(r'\\d*\\.?\\d+',obj['fund_manager_years_of_experience_with_fund'])[-1]"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}