{
    "website": "https://fundsus.dws.com",
    "spider": "financial_detail",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "fundsus.dws.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://fundsus.dws.com/us/en-us/products/mutual-funds/dws-california-tax-free-income-fund.html/S"
        ],
        "ext_codes": {
            "share_class": {
                "paths": [
                    "//a[@id=\"dropdown-categories\"]//span[1]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "asset_class": {
                "paths": [
                    "//p[contains(text(),'Asset Class')]//following-sibling::h2//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//div[@class=\"fundFacts\"]//p[contains(text(),'Ticker')]//following-sibling::h2//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//div[@class=\"fundFacts\"]//p[contains(text(),'CUSIP')]//following-sibling::h2//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "instrument_name": {
                "paths": [
                    "//h1[@class=\"fund-name\"]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity": {
                "paths": [
                    "//p[contains(text(),'Effective Maturity')]//following-sibling::h2//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity_as_of_date": {
                "paths": [
                    "//div[contains(text(),'Fund Facts')]//following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[contains(text(),'Management Team')]//parent::div"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//p[1]//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    ".//p[2]//text()"
                ],
                "cleanup_functions": [
                    "obj['fund_manager_years_of_experience_in_industry'].split(\" \")[0].strip()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "benchmarks": {
                "paths": [
                    "//div[@class=\"row performance-return-table\"]//div[@class=\"row no-padding fund-table-row\"]//span[contains(text(),'Index')]//text()"
                ],
                "cleanup_functions": [
                    "list[set(obj['benchmarks'])]"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//*[@id='fund-detail-table']//span[contains(text(),'Gross')]//following::div[position()=1]//span//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//*[@id='fund-detail-table']//span[contains(text(),'Net')]//following::div[position()=1]//span//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//div[@class=\"fundFacts\"]//p[contains(text(),'Share Class Inception')]//following-sibling::h2//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//p[contains(text(),'Total Net Assets')]//following-sibling::h2//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//div[contains(text(),'Fund Facts')]//following-sibling::span//text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}