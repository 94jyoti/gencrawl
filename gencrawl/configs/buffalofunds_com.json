{
    "website": "https://buffalofunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "buffalofunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://buffalofunds.com/discovery/,https://buffalofunds.com/early-stage-growth/",
            "https://buffalofunds.com/growth/",
            "https://buffalofunds.com/international/",
            "https://buffalofunds.com/large-cap/",
            "https://buffalofunds.com/mid-cap/",
            "https://buffalofunds.com/small-cap/",
            "https://buffalofunds.com/dividend-focus/",
            "https://buffalofunds.com/flexible-income/",
            "https://buffalofunds.com/high-yield/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[@class='entry-title']//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "(//table[starts-with(@id, 'tablepress-')])[1]/tbody/tr[1]/td[text()!='']"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//b[contains(text(),'Ticker')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//b[contains(text(),'Total Net Asset')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//b[contains(text(),'Total Net Asset')]/parent::td/following-sibling::td/span/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace(\"(\",\"\").replace(\")\",\"\")"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//b[contains(text(),'Expense Ratio')]/parent::td/following-sibling::td//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//b[contains(text(),'Inception Date')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h5[contains(text(),'Fund Objective')]/parent::div/parent::div/following-sibling::div/div/p//text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "average_modified_duration": {
                "paths": [
                    "//span[contains(text(),'Average Duration')]/parent::b/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity": {
                "paths": [
                    "//b[contains(text(),'Average Maturity')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity_as_of_date": {
                "paths": [
                    "//h5[contains(text(),'Portfolio Characteristics')]/parent::div/parent::div/following-sibling::div//table/thead/tr/th/span/text()"
                ],
                "cleanup_functions": [
                    "obj[\"average_weighted_maturity_as_of_date\"].replace(\"(\",\"\").replace(\")\",\"\")"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//b[contains(text(),'3-Yr Annualized')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h5[contains(text(),'Portfolio Characteristics')]/parent::div/parent::div/following-sibling::div//table/thead/tr/th[1]//span//text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].replace(\"(\",\"\").replace(\")\",\"\")"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency": {
                "paths": [
                    "//b[contains(text(),'Dividend Distribution')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//b[contains(text(),'30-day SEC ')]/parent::td/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "//h5[contains(text(),'Portfolio Characteristics')]/parent::div/parent::div/following-sibling::div//table/thead/tr/th//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//b[contains(text(),'Benchmark')]/parent::td/following-sibling::td/span/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_capital_gains": {
                "paths": [
                    "//table[@id='tablepress-207']"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h2[contains(text(),'Management')]/parent::div/parent::div/parent::div/parent::div/following-sibling::div/div/div[2]/div"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./b/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    "./p/text()"
                ],
                "cleanup_functions": [
                    "\" \".join(obj['fund_manager_years_of_experience_in_industry'].split('Years'))"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}