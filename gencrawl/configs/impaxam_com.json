{
    "website": "https://impaxam.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "impaxam.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://impaxam.com/products/us-equities/large-cap-strategy/pax-large-cap-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1[@class = 'module__header__title']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//select[@name = 'ClassSelect']/option"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//div[contains(text(),'Symbol')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//div[contains(text(),'CUSIP')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_consultant": {
                "paths": [
                    "//p[contains(text(),'is investment adviser')]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_consultant'].split(',')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Assets (all share classes)')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h3[contains(text(),'Portfolio Statistics')]/following-sibling::div/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('of')[-1].split(')')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//div[contains(text(),'Sales Charge')]/following-sibling::div/text()"
                ],
                "cleanup_functions": [
                    "[f.split(',')[-1].split(':')[-1] for f in obj['fees_total_12b_1']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//div[contains(text(),'Sales Charge')]/following-sibling::div/text()"
                ],
                "cleanup_functions": [
                    "[m.split(',')[0] for m in obj['maximum_sales_charge_full_load']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//div[contains(text(),'Gross of Fees')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//div[contains(text(),'Net of Fees')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//div[contains(text(),'Launch Date')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//div[contains(text(),'Minimum Investment')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//td[contains(text(),'Turnover Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//td[contains(text(),'Turnover Ratio')]/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].split(')')[0].split('(')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//td[contains(text(),'Index')]/text()"
                ],
                "cleanup_functions": [
                    "list(set(obj['benchmarks']))"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h2[contains(text(),'Portfolio Management')]/parent::div/following-sibling::div/div/div/article/div/div[2]/h3"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}