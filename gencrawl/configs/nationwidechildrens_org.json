{
    "website": "http://nationwidechildrens.org",
    "domain": "hospital",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "nationwidechildrens.org"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.nationwidechildrens.org/find-a-doctor/profiles/elizabeth-e-short",
            "https://www.nationwidechildrens.org/find-a-doctor/profiles/wahida-b-abaza",
            "https://www.nationwidechildrens.org/find-a-doctor/profiles/collin-j-abbott",
            "https://www.nationwidechildrens.org/find-a-doctor/profiles/mary-ann-abrams",
            "https://www.nationwidechildrens.org/find-a-doctor/profiles/rolla-f-abu-arja",
            "https://www.nationwidechildrens.org/find-a-doctor/profiles/john-p-ackerman-2",
            "https://www.nationwidechildrens.org/find-a-doctor/profiles/stevie-annpeiran-adams",
            "https://www.nationwidechildrens.org/find-a-doctor/profiles/brent-h-adler",
            "https://www.nationwidechildrens.org/find-a-doctor/profiles/oliver-f-adunka",
            "https://www.nationwidechildrens.org/find-a-doctor/profiles/sara-m-agosta"
        ],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "address": {
                "paths": [
                    "//*[contains(text(), 'Contact Information')]/following-sibling::ul/li[last()]//text()"
                ],
                "cleanup_functions": [
                    "obj['address'].replace('(','').replace('map','').replace(')','')"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "phone": {
                "paths": [
                    "//*[contains(text(), 'Contact Information')]/following::li[1]/a/@href"
                ],
                "cleanup_functions": [
                    "obj['phone'].replace('tel','').replace(':','')"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}