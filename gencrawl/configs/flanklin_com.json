{
    "website": "https://www.franklintempleton.com",
    "spider": "financial_detail",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "www.franklintempleton.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.franklintempleton.com/investor/investments-and-solutions/investment-options/mutual-funds/overview/656/franklin-arizona-tax-free-income-fund/FAZZX"
        ],
        "pagination":{
            "pagination": {
                "paths": [
                    "//button[@id='share-class']//following-sibling::ul//li//a//@href"
                ],
                "return_type": "list",
                "cleanup_functions": 
                    ["['https://www.franklintempleton.com'+i for i in obj['pagination']]"]
            }
        },
        
        "ext_codes": {
            
            "share_class": {
                "paths": [
                    "//button[@id='share-class']//text()"
                ],
                "return_type": "str",
                "selector":"root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//b[@class='symbol']//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//th[contains(text(),'CUSIP')]//following-sibling::td//b//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "instrument_name": {
                "paths": [
                    "//h1[@class='header-fund']//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//th[contains(text(),'12b-1 Fee')]//following-sibling::td//b//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Gross Expense Ratio')]//following-sibling::td//b//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//th[contains(text(),'Net Expense Ratio')]//following-sibling::td//b//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//th[contains(text(),'Inception Date')]//following-sibling::td//b//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers":{
             "paths":["//div[@id='fund-management-scroll']//div[contains(@class,'media-body')]"], 
             "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//h3//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_with_fund": {
                "paths": [
                    ".//ul//li[2]//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "dividend_frequency":{
             "paths":["//th[contains(text(),'Distributions')]/following-sibling::td//b//text()"],
              "return_type": "str",
                "selector": "root"	
            
            },
            "initial_sales_charge":{
             "paths":["//th[contains(text(),'Max Initial Sales Charge')]//following-sibling::td//b//text()"],
             "return_type": "str",
                "selector": "root"	
            
            },
            "contingent_deferred_sales_charge":{
             "paths":["//th[contains(text(),'CDSC')]//following-sibling::td//b//text()"],
              "return_type": "str",
              "selector": "root"	 
            },
            "temp_investment_strategyyy":{
             "paths":["//h3[contains(text(),'Strategy')]//following-sibling::ul//li","//h3[contains(text(),'Strategy')]//following-sibling::p//text()"],
             "child_return_strategy": "single_item",
                "return_type": "selector",
                "selector": "root"
            
            },
            "minimum_initial_investment":{
             "paths":["//h3[contains(text(),'Minimum Investment')]//following-sibling::p//nobr//text()","//h3[contains(text(),'Minimum Investment')]//following-sibling::a//text()"],
             "return_type": "str",
             "selector": "root"	
            
            },
            "sec_yield_date_30_day":{
             "paths":["//th[contains(text(),'30-Day Standardized yield')]//small//text()"],
             "return_type": "str",
             "selector": "root"	,
              "cleanup_functions": 
                    [" obj['sec_yield_date_30_day'].replace('(updated monthly)','')"]
             
            },
            "sec_yield_without_waivers_date_30_day":{
             "paths":["//th[contains(text(),'30-Day Standardized yield')]//small//text()"],
             "return_type": "str",
             "selector": "root"	,
             "cleanup_functions": 
                    [" obj['sec_yield_without_waivers_date_30_day'].replace('(updated monthly)','')"]
            },
            "sec_yield_30_day":{
             "paths":["//th[contains(text(),'30-Day Standardized yield')]//following-sibling::td//b//text()","//th[contains(text(),'30-Day Standardized yield')]//following::th[contains(text(),'w/ Waiver')]//following-sibling::td//b//text()"],
             "return_type": "str",
             "selector": "root"	
             
            
            },
            "sec_yield_without_waivers_30_day":{
             "paths":["//th[contains(text(),'30-Day Standardized yield')]//following::th[contains(text(),'w/o Waiver')]//following-sibling::td//b//text()"],
             "return_type": "str",
             "selector": "root"	
            
            }
        }
    }
}