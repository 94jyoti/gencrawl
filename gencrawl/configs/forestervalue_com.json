{
    "website": "http://www.forestervalue.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "forestervalue.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "http://www.forestervalue.com/valuefund/characteristics.html"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//tbody//tr//p[@class='headline-serif']/text()"
                ],
                "cleanup_functions": [
                    "obj['instrument_name'].replace('Characteristics', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//td[contains(text(),'Symbol')]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//td[contains(text(),'Symbol')]/text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('(')[-1] "
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Total Assets (mil)')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "(//td[contains(text(),'Expense Ratio')]/following-sibling::td)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
			"minimum_initial_investment":{
			 "paths":["//td[contains(text(),'Minimum Investment')]/following-sibling::td[1]/text()"],
			 "return_type": "str",
             "selector": "root"
			
			}
        }
    }
}