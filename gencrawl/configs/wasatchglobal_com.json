{
    "website": "https://wasatchglobal.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "wasatchglobal.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://wasatchglobal.com/wasatch-core-growth-fund-investor/",
            "https://wasatchglobal.com/wasatch-micro-cap-fund-investor/",
            "https://wasatchglobal.com/wasatch-micro-cap-value-fund-investor/",
            "https://wasatchglobal.com/wasatch-small-cap-growth-fund-investor/",
            "https://wasatchglobal.com/wasatch-small-cap-value-fund-investor/",
            "https://wasatchglobal.com/wasatch-ultra-growth-fund-investor/",
            "https://wasatchglobal.com/wasatch-hoisington-us-treasury-fund/",
            "https://wasatchglobal.com/wasatch-emerging-india-fund-investor/",
            "https://wasatchglobal.com/wasatch-emerging-markets-select-fund-investor/",
            "https://wasatchglobal.com/wasatch-emerging-markets-small-cap-fund-investor/",
            "https://wasatchglobal.com/wasatch-frontier-emerging-small-countries-fund-investor/",
            "https://wasatchglobal.com/wasatch-greater-china-fund-investor/",
            "https://wasatchglobal.com/wasatch-international-growth-fund-investor/",
            "https://wasatchglobal.com/wasatch-international-opportunities-fund-investor/",
            "https://wasatchglobal.com/wasatch-international-select-fund-investor/",
            "https://wasatchglobal.com/wasatch-global-opportunities-fund-investor/",
            "https://wasatchglobal.com/wasatch-global-select-fund-investor/",
            "https://wasatchglobal.com/wasatch-global-value-fund-investor/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h2/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//div[contains(@class,\"fund-summary__item\")]/*[contains(text(),'Ticker')]/following-sibling::span"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "(//span[contains(text(),'CUSIP')]//following::span)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//select[@id='investor-dropdown']/option/text()"
                ],
                "cleanup_functions": [
                    "[o for o in obj['share_class'] if (o.lower() in obj['fund_url'] or len(obj['share_class']) == 1)][0]"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//span[contains(text(),'Total Assets')]/following::span[@id=\"total-assets\"]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "(//span[contains(text(),'Gross Expense Ratio')]//following::span)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "(//span[contains(text(),'Net Expense Ratio*')]//following::span)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//span[contains(text(),'Inception Date')]/following::span[@id=\"inception-date\"]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h5[contains(text(),'Objective')]//following-sibling::p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration": {
                "paths": [
                    "//*[contains(text(),'Effective Duration (Years)')]/following::span[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration_date": {
                "paths": [
                    "//*[contains(text(),'Characteristics')]/following::span[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity": {
                "paths": [
                    "//*[contains(text(),'Weighted Average Maturity')]/following::span[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity_as_of_date": {
                "paths": [
                    "//*[contains(text(),'Characteristics')]/following::span[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "(//span[contains(text(),'Min. Initial Investment')]//following::span)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//*[contains(text(),'Portfolio Turnover (%)')]/following::span[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//*[contains(text(),'Characteristics')]/following::span[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "(//span[contains(text(),'Primary Benchmark')]//following::span)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class=\"fund-managers__item\"]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//span[@class='fund-managers__name']/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    ".//div[@class='fund-managers__facts__item'][2]/span[1]/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_with_fund": {
                "paths": [
                    ".//div[@class='fund-managers__facts__item'][1]/span[1]/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        },
        "pagination": {
            "pagination": {
                "paths": [
                    "//div[@id='investor-dropdown-wrapper']//div[@class='selectize-dropdown-content']/div/@data-value"
                ],
                "cleanup_functions": [
                    "[(obj['fund_url'].rsplit('-', 1)[0] + '-' + i + '/') for i in obj['pagination']]"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}