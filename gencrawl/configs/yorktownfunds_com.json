{
    "website": "https://yorktownfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "yorktownfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://yorktownfunds.com/funds-performance/short-term-bond-fund/",
            "https://yorktownfunds.com/funds-performance/multi-asset-fund/",
            "https://yorktownfunds.com/funds-performance/growth-fund/",
            "https://yorktownfunds.com/funds-performance/small-cap-fund-2/",
            "https://yorktownfunds.com/funds-performance/capital-income-fund/",
            "https://yorktownfunds.com/funds-performance/master-allocation-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class='wpb_wrapper']/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//th[contains(text(),'Ticker:')]/following-sibling::td"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "//th[contains(text(),'Class:')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//th[contains(text(),'Assets (3/31/21)')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//th[contains(text(),'Assets (3/31/21)')]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace('Assets', \"\").replace(\"(\",\"\").replace(\")\",\"\").replace(\":\",\"\")"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//th[contains(text(),'Objective')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h2[contains(text(),'Investment Strategy')]/following-sibling::p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "//th[contains(text(),'Distributions')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//div[contains(text(),'30-Day SEC Yields ')]/following::table[1]//tr[position()>1]//td[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "//div[contains(text(),'30-Day SEC Yields ')]/small/text()"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_date_30_day'].replace('as of',\"\")"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_30_day": {
                "paths": [
                    "//div[contains(text(),'30-Day SEC Yields ')]/following::table[1]//tr[position()>1]//td[2]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "sec_yield_without_waivers_date_30_day": {
                "paths": [
                    "//div[contains(text(),'30-Day SEC Yields ')]/small/text()"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_without_waivers_date_30_day'].replace('as of', \"\")"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//th[contains(text(),'Benchmark:')]/following-sibling::td/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h3[contains(text(),'Portfolio Manager')]/parent::div/p//a"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}