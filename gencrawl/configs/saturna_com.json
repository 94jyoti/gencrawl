{
    "website": "https://www.saturna.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "saturna.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_saturna_com",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.saturna.com/amana/income-fund",
            "https://www.saturna.com/amana/growth-fund",
            "https://www.saturna.com/amana/developing-world-fund",
            "https://www.saturna.com/amana/participation-fund",
            "https://www.saturna.com/sextant/growth-fund",
            "https://www.saturna.com/sextant/international-fund",
            "https://www.saturna.com/sextant/core-fund",
            "https://www.saturna.com/sextant/global-high-income-fund",
            "https://www.saturna.com/sextant/short-term-bond-fund",
            "https://www.saturna.com/sextant/bond-income-fund",
            "https://www.saturna.com/sustainable/equity-fund",
            "https://www.saturna.com/sustainable/bond-fund",
            "https://www.saturna.com/idaho",
            "https://www.saturna.com/sustainable/bond-fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h2/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//h6[contains(text(),'Share Class')]/parent::span/following-sibling::span/label"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//h6[contains(text(),'Ticker')]/parent::span/following-sibling::span/label//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_net_assets":
            {
             "paths":["//h6[contains(text(),'AUM')]/parent::span/following-sibling::span/label//text()"],
             "return_type": "list-map",
             "selector": "root"
            },
            "total_net_assets_date":{
            "paths":["//div[@id='fund-facts-app']/div//p//text()"],
            "return_type":"str",
            "selector":"root"
            },
            
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "annual_fund_operating_expenses_after_fee_waiver": {
                "paths": [
                    "//strong[contains(text(),'Total Annual Fund Operating Expenses')]/parent::td/following-sibling::td//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//td[contains(text(),'12b-1')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "other_expenses": {
                "paths": [
                    "//td[contains(text(),'Other Expenses')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//td[contains(text(),'Management Fees')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h3[contains(text(),'Investment Objective')]/parent::div/p[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//*[contains(text(), 'Principal Investment Strategies')]/following-sibling::p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "temp_capital_gains": {
                "paths": [
                    "//table[contains(@class, 'distributions-table')]/tbody/tr[not(@class) or @class!='subtitle']"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "cg_ex_date": {
                "paths": [
                    "./td[2]"
                ],
                "return_type": "str",
                "selector": "temp_capital_gains"
            },
            "cg_record_date": {
                "paths": [
                    "./td[1]"
                ],
                "return_type": "str",
                "selector": "temp_capital_gains"
            },
            "cg_pay_date": {
                "paths": [
                    "./td[2]"
                ],
                "return_type": "str",
                "selector": "temp_capital_gains"
            },
            "short_term_per_share": {
                "paths": [
                    "./td[5]"
                ],
                "return_type": "str",
                "selector": "temp_capital_gains"
            },
            "long_term_per_share": {
                "paths": [
                    "./td[6]"
                ],
                "return_type": "str",
                "selector": "temp_capital_gains"
            },
            "total_per_share": {
                "paths": [
                    "./td[7]"
                ],
                "return_type": "str",
                "selector": "temp_capital_gains"
            },
            "temp_dividends": {
                "paths": [
                    "//table[contains(@class, 'distributions-table')]/tbody/tr[not(@class) or @class!='subtitle']"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "ordinary_income": {
                "paths": [
                    "./td[3]"
                ],
                "return_type": "str",
                "selector": "temp_dividends"
            },
            "qualified_income": {
                "paths": [
                    "./td[4]"
                ],
                "return_type": "str",
                "selector": "temp_dividends"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h2[contains(text(),'Portfolio Managers')]/parent::div/div/div/div[2]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./span/a//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_with_fund": {
                "paths": [
                    "./span/text()"
                ],
                "cleanup_functions": [
                    "'since'+obj['fund_manager_years_of_experience_with_fund'].split('since')[-1]"
                ],
                "return_type": "join",
                "selector": "temp_fund_managers"
            },
            "sec_yield_30_day":{
             "paths":["//h6[contains(text(),'30-Day Yield')]/parent::span/following-sibling::span/label//text()"],
             "return_type": "list-map",
            "selector": "root"
            },
            "sec_yield_date_30_day":{
             "paths":["//div[@id='fund-facts-app']//div//p//text()"],
             "return_type":"join",
             "selector":"root"
            
            
            },
            "total_expense_gross": {
                "paths": [
                    "//strong[normalize-space(text())='Total Annual Fund Operating Expenses']/parent::td/following-sibling::td//strong//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net":{
            "paths": [
                    "//strong[contains(text(),'Total Annual Fund Operating Expenses after Fee Waiver and Expense Reimbursement')]/parent::td/following-sibling::td//strong//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            
            },
            "benchmarks":{
             "paths":["//div[@class='short-term table-responsive']//tbody//tr//td[2][string-length() = 0] //preceding-sibling::td//text()"],
             "return_type":"list",
            "selector":"root"
            
            },
            "shareholder_service_fees":{
             "paths":["//strong[contains(text(),'Shareowner Fees')]/parent::p/following-sibling::p[1]//text()"],
             "return_type":"str",
            "selector":"root"
            
            }
        }
    }
}