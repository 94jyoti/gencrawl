{
    "website": "https://www.olsteinfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "olsteinfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.olsteinfunds.com/funds/olstein-strategic-opportunities-fund/olstein-strategic-oportunities-fund-overview",
            "https://www.olsteinfunds.com/funds/olstein-all-cap-value-fund/olstein-all-cap-value-fund-overview"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h2[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/following-sibling::td"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//tr[@class='header-cols']/th/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Net Assets')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "(//h4[@class='center']/following-sibling::div/div/text())[1]"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('As of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "initial_sales_charge": {
                "paths": [
                    "//td[contains(text(),'Maximum Initial Sales Load')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "contingent_deferred_sales_charge": {
                "paths": [
                    "//td[contains(text(),'Maximum CDSC')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//td[contains(text(),'Redemption Fees')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[text()='Gross Expense Ratio']/following-sibling::td/text()",
                    "//td[text()='Expense Ratio']/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Net Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Initial Investment')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//td[contains(text(),'Portfolio Turnover')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "(//h4[@class='center']/following-sibling::div/div/text())[1]"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].split('As of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}