{
    "website": "https://www.amitahealth.org/",
    "domain": "hospital",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "amitahealth.org"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.amitahealth.org/provider/dr-john-johnson-md-anesthesiology"
        ],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "//div[@class=\"form-group ih-field-providername\"]/div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "speciality": {
                "paths": [
                    "//div[@class=\"form-group ih-field-primaryspecialty\"]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "affiliation": {
                "paths": [
                    "//div[@class=\"form-group ih-field-dynamiccol_facilities\"]/div/ul"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "address_raw": {
                "paths": [
                    "//div[@class=\"form-group ih-field-primarylocationcompleteaddress\"]/div"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "practice_name": {
                "paths": [
                    "//div[@class=\"form-group ih-field-primarylocationname\"]/div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "phone": {
                "paths": [
                    "(//div[@class=\"form-group ih-field-primarylocationcompleteaddress\"]/following-sibling::div)[2]//a/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fax": {
                "paths": [
                    "(//div[@class=\"form-group ih-field-primarylocationcompleteaddress\"]/following-sibling::div)[3]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}