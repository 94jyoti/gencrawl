{
    "website": "https://www.barings.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "barings.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_barings_com",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.barings.com/us/institutional/funds/mutual-funds/barings-active-short-duration-bond-fund",
            "https://www.barings.com/us/institutional/funds/mutual-funds/barings-emerging-markets-debt-blended-total-return",
            "https://www.barings.com/us/institutional/funds/mutual-funds/barings-global-credit-income-opportunities-fund",
            "https://www.barings.com/us/institutional/funds/mutual-funds/barings-global-emerging-markets-equity-fund",
            "https://www.barings.com/us/institutional/funds/mutual-funds/barings-global-floating-rate-fund",
            "https://www.barings.com/us/institutional/funds/mutual-funds/barings-us-high-yield-fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//span[@class='disclaimer']/parent::h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//h2[contains(text(),'Symbol')]/text()"
                ],
                "cleanup_functions": [
                    "obj['nasdaq_ticker'].split(':')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//h2[contains(text(),'CUSIP')]/text()"
                ],
                "cleanup_functions": [
                    "obj['cusip'].split(':')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//select[@id='share_class']/option[@selected]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Expense Ratio (Gross)')]/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Expense Ratio (Net)')]/following-sibling::td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "(//td[contains(text(),'Inception Date')]/following-sibling::td)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h2[contains(text(),'Objective')]/following-sibling::p[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h2[contains(text(),'Strategy')]/following-sibling::ul/li/text()",
                    "//h2[contains(text(),'Strategy')]/following-sibling::p[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "weighted_average_duration": {
                "paths": [
                    "//strong[contains(text(),'Weighted Average Duration')]/parent::span/parent::div/following-sibling::div/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "weighted_average_duration_as_of_date": {
                "paths": [
                    "//strong[contains(text(),'Weighted Average Duration')]/following-sibling::em/text()"
                ],
                "cleanup_functions": [
                    "obj['weighted_average_duration_as_of_date'].split('as of')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//td[contains(text(),'Sub. SEC 30-day Yield')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "//h2[contains(text(),'Fund Yield')]/following-sibling::span/text()"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_date_30_day'].split('as of')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_30_day": {
                "paths": [
                    "//td[contains(text(),'Unsub. SEC 30-day Yield')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_date_30_day": {
                "paths": [
                    "//h2[contains(text(),'Fund Yield')]/following-sibling::span/text()"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_without_waivers_date_30_day'].split('as of')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "distribution_yield_12_month": {
                "paths": [
                    "//td[contains(text(),'Distribution Yield (at NAV)')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h1[contains(text(),'Portfolio Managers')]/parent::div/div/div/div/div/div/a/following-sibling::h4"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        },
        "pagination": {
            "pagination": {
                "paths": [
                    "//select[@id='share_class']/option[not(@selected)]/@value"
                ],
                "cleanup_functions": [
                    "[obj['fund_url']+ '/' + p for p in obj['pagination']]"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}