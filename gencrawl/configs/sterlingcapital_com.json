{
    "website": "https://sterlingcapital.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "sterlingcapital.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://sterlingcapital.com/funds/equity-funds/behavioral-international-equity-fund/",
            "https://sterlingcapital.com/funds/equity-funds/behavioral-large-cap-value-equity-fund/",
            "https://sterlingcapital.com/funds/equity-funds/behavioral-small-cap-value-equity-fund/",
            "https://sterlingcapital.com/funds/equity-funds/equity-income-fund/",
            "https://sterlingcapital.com/funds/equity-funds/mid-value-fund/",
            "https://sterlingcapital.com/funds/equity-funds/smid-opportunities-fund/",
            "https://sterlingcapital.com/funds/equity-funds/special-opportunities-fund/",
            "https://sterlingcapital.com/funds/equity-funds/stratton-mid-cap-value-fund/",
            "https://sterlingcapital.com/funds/equity-funds/stratton-real-estate-fund/",
            "https://sterlingcapital.com/funds/equity-funds/stratton-small-cap-value-fund/",
            "https://sterlingcapital.com/funds/fixed-income-funds/corporate-fund/",
            "https://sterlingcapital.com/funds/fixed-income-funds/intermediate-u-s-government-fund/",
            "https://sterlingcapital.com/funds/fixed-income-funds/north-carolina-intermediate-tax-free-fund/",
            "https://sterlingcapital.com/funds/fixed-income-funds/quality-income-fund/",
            "https://sterlingcapital.com/funds/fixed-income-funds/short-duration-bond-fund/",
            "https://sterlingcapital.com/funds/fixed-income-funds/south-carolina-intermediate-tax-free-fund/",
            "https://sterlingcapital.com/funds/fixed-income-funds/total-return-bond-fund/",
            "https://sterlingcapital.com/funds/fixed-income-funds/ultra-short-bond-fund/",
            "https://sterlingcapital.com/funds/fixed-income-funds/virginia-intermediate-tax-free-fund/",
            "https://sterlingcapital.com/funds/fixed-income-funds/west-virginia-intermediate-tax-free-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "(//h2)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//span[contains(text(),'Type')]/parent::th/following-sibling::th"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//th[contains(text(),'Ticker')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].replace('Shares', '')"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "contingent_deferred_sales_charge": {
                "paths": [
                    "//th[contains(text(),'Max. Deferred Sales Charge')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//th[contains(text(),'Max. Up Front Sales Charge')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//th[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "effective_duration": {
                "paths": [
                    "(//td[contains(text(),'Effective Duration')]/parent::tr)[1]/td[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration_date": {
                "paths": [
                    "//h4[contains(text(),'Summary Statistics')]/text()"
                ],
                "cleanup_functions": [
                    "obj['effective_duration_date'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//th[contains(text(),'Inception Date')]/parent::tr/following-sibling::tr[1]/th/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//th[contains(text(),'Subsequent Investment Min')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//td[contains(text(),'Annual Turnover')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h4[contains(text(),'Summary Statistics')]/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//h4[contains(text(),'30-Day SEC Yield')]/parent::div/following-sibling::div/table/thead/following-sibling::tbody/tr/td[3]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "//h4[contains(text(),'30-Day SEC Yield')]"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_date_30_day'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_30_day": {
                "paths": [
                    "//h4[contains(text(),'30-Day SEC Yield')]/parent::div/following-sibling::div/table/thead/following-sibling::tbody/tr/td[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_date_30_day": {
                "paths": [
                    "//h4[contains(text(),'30-Day SEC Yield')]"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_without_waivers_date_30_day'].split('as of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@id='management']/div/following-sibling::div//div/div/div/h5"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}