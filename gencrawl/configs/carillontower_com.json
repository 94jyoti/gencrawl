{
    "website": "www.carillontower.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "carillontower.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_filed_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.carillontower.com/our-funds/fund-strategies/clarivest/capital-appreciation-fund",
            "https://www.carillontower.com/our-funds/fund-strategies/eagle/growth-and-income-fund",
            "https://www.carillontower.com/our-funds/fund-strategies/scout/mid-cap-fund",
            "https://www.carillontower.com/our-funds/fund-strategies/eagle/mid-cap-growth-fund",
            "https://www.carillontower.com/our-funds/fund-strategies/eagle/small-cap-growth-fund",
            "https://www.carillontower.com/our-funds/fund-strategies/scout/small-cap-fund",
            "https://www.carillontower.com/our-funds/fund-strategies/reams/core-bond-fund",
            "https://www.carillontower.com/our-funds/fund-strategies/reams/core-plus-bond-fund",
            "https://www.carillontower.com/our-funds/fund-strategies/reams/unconstrained-bond-fund",
            "https://www.carillontower.com/our-funds/fund-strategies/scout/international-fund",
            "https://www.carillontower.com/our-funds/fund-strategies/clarivest/international-stock-fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class='headerimage']/div/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//b[contains(text(),'Symbol')]/parent::p/parent::td/parent::tr/following-sibling::tr"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./td[2]/p/text()"
                ],
                "return_type": "list",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "./td[3]/p/text()"
                ],
                "return_type": "list",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "./td[1]/p/text()"
                ],
                "return_type": "list",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Total net assets')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//th[contains(text(),'Fund Summary ')]/small"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace('as of', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//b[contains(text(),'Symbol')]/parent::p/parent::td/parent::tr/following-sibling::tr/td[4]/p/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Initial minimum investment')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//td[contains(text(),'Benchmark')]/following-sibling::td/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h3[contains(text(),'Investment Team')]/following-sibling::div/section//div/div/div/div/div/div/div"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./h2/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    "./p[2]/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}