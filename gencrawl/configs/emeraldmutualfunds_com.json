{
    "website": "https://www.emeraldmutualfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "emeraldmutualfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.emeraldmutualfunds.com/emerald-growth-fund",
            "https://www.emeraldmutualfunds.com/emerald-banking-finance-fund",
            "https://www.emeraldmutualfunds.com/emerald-insights-fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    ".//*[@id=\"dailyPriceTabs\"]//li"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    ".//*[text()='Ticker:']//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    ".//*[text()='CUSIP:']//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "total_expense_gross": {
                "paths": [
                    ".//*[text()='Gross Expense Ratio']//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    ".//*[text()='Net Expense Ratio:']//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    ".//*[text()='Inception Date:']//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h4[contains(text(),'Objective')]//following-sibling::P[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    ".//*[text()='Minimum Investment:']//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "(.//*[@data-performance-name=\"Benchmark\"])[1]//td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    ".//*[@id=\"bioTarget\"]//h4"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}