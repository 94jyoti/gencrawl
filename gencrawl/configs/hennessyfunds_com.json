{
    "website": "https://www.hennessyfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "hennessyfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.hennessyfunds.com/funds/cornerstone-growth",
            "https://www.hennessyfunds.com/funds/focus",
            "https://www.hennessyfunds.com/funds/cornerstone-midcap",
            "https://www.hennessyfunds.com/funds/cornerstone-large-growth",
            "https://www.hennessyfunds.com/funds/cornerstone-value",
            "https://www.hennessyfunds.com/funds/total-return",
            "https://www.hennessyfunds.com/funds/equity-income",
            "https://www.hennessyfunds.com/funds/balanced",
            "https://www.hennessyfunds.com/funds/energy",
            "https://www.hennessyfunds.com/funds/midstream",
            "https://www.hennessyfunds.com/funds/gas",
            "https://www.hennessyfunds.com/funds/japan",
            "https://www.hennessyfunds.com/funds/japan-small-cap",
            "https://www.hennessyfunds.com/funds/large-cap-financial",
            "https://www.hennessyfunds.com/funds/small-cap-financial",
            "https://www.hennessyfunds.com/funds/tech"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class='fund__main']//div/h2/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//div[@class=\"section__row section__row--compact\"]//following-sibling::h4"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./following::div[1]//span//text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Total Fund Assets')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h2[contains(text(),'Portfolio Characteristics')]/parent::div/following-sibling::div/span/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].replace('as of', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//h4[contains(text(),'Gross expense ratio')]/parent::div/following-sibling::div/h4/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//h4[contains(text(),'Net expense ratio')]/parent::div/following-sibling::div/h4/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "(//td[contains(text(),'Inception Date ')]/following-sibling::td)/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//td[contains(text(),'Portfolio Turnover')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "//h2[contains(text(),'Portfolio Characteristics')]/parent::div/following-sibling::div/span/text()"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].replace('as of', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class=\"section__row\"]/ul/li/div/div/div/div/h4"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    "./parent::div/parent::div/div/ul/li/div/div/div/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_with_fund": {
                "paths": [
                    "./parent::div/parent::div/div/ul/li/div/div/div/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}