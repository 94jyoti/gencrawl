{
    "website": "https://www.virtus.com",
    "spider": "financial_detail",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "www.virtus.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "custom_settings":{
          "DUPEFILTER_CLASS" : "scrapy.dupefilters.BaseDupeFilter"
        },
        "crawl_method": "SELENIUM",
        "wait_time": 5,
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.virtus.com/products/allianzgi-convertible#shareclass.INST/period.daily"
            
        ],
        "ext_codes": {
            "share_class": {
                "paths": [
                    "//select[@id = 'share-class']/option[1]/text()"
                ],
                "cleanup_functions": [
                    "obj['fund_url'].split('#shareclass.')[-1].split('/')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "instrument_name":{
             "paths":["//div[@class = 'product-name']/h1/text()"],
             "return_type": "str",
             "selector":"root"
            },
            
            "nasdaq_ticker": {
                "paths": [
                    "//span[@class = 'symbol']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//span[@class = 'cusip']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class = 'mgr-info']"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./h4/text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    "./p[2]/text()"
                ],
                "cleanup_functions": [
                    "obj['fund_manager_years_of_experience_in_industry'].split(':')[-1]"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers",
                "comment": "//div[@class = 'mgr-info']/h4/following-sibling::p[2]"
            },
            "fund_manager_years_of_experience_with_fund": {
                "paths": [
                    "./p[2]/text()"
                ],
                "cleanup_functions": [
                    "obj['fund_manager_years_of_experience_with_fund'][-1]",
                    "obj['fund_manager_years_of_experience_with_fund'].split(':')[-1]"
                ],
                "return_type": "list",
                "selector": "temp_fund_managers"
            },
            "benchmarks": {
                "paths": [
                    "//dt[@class = 'share-index1-name share-name']/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//span[@class = 'inception']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            
            },
            "sub_advisor":{
             "paths":["//h3[contains(text(),'Investment Partner')]/parent::div/following-sibling::div//h4//text()"],
             "return_type": "list",
            "selector": "root"
            
            
            },
            "portfolio_assets": {
                "paths": [
                    "//span[@class = 'assets-by-fund']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "(//span[@class = 'assets-asof'])[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets": {
                "paths": [
                    "//span[@class = 'assets-by-class']//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "(//span[@class = 'assets-asof'])[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//span[@class = 'Yield_SEC']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "//span[@class = 'AsOfDate_DstrbYld']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_30_day": {
                "paths": [
                    "//span[@class = 'Yield_SEC_Gross']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_date_30_day": {
                "paths": [
                    "//span[@class = 'AsOfDate_DstrbYld']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "asset_class":{
             "paths":["//span[@class = 'product-type-name']/following-sibling::span[1]//text()"],
             "return_type": "str",
                "selector": "root"
            
            }
        }
    }
}