{
    "website": "https://jspartners.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "jspartners.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://jspartners.com/fund/global-growth-fund/",
            "https://jspartners.com/fund/international-growth-fund/",
            "https://jspartners.com/fund/large-cap-growth-fund/",
            "https://jspartners.com/fund/select-20-growth-fund/",
            "https://jspartners.com/fund/smid-cap-growth-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h2/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//div[@class='fe_row fe_header']/div[contains(@class,'class')]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//div[@class='fe_row fe_header']/div/text()"
                ],
                "cleanup_functions": [
                    "[o.split('Class')[-1] for o in obj['nasdaq_ticker']]",
                    "[o.split('(')[0] for o in obj['nasdaq_ticker']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split('Class')[0] "
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//div[contains(text(),'Total Assets Under Management')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "(//div[contains(text(),'As of')]/text())[1]"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('As of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "other_expenses": {
                "paths": [
                    "//div[contains(text(),'Other Expenses')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//div[contains(text(),'Management Fees')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "shareholder_service_fees": {
                "paths": [
                    "//div[contains(text(),'Shareholder Servicing Fee')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "expense_waivers": {
                "paths": [
                    "//div[contains(text(),'Fee Waiver')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//div[@class='fe_row total_row'][1]/div[not(contains(text(),'Total'))]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//div[contains(text(),'Total After Waiver')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h3[contains(text(),'Objective')]/following-sibling::div/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//div[contains(text(),'Minimum Investment')]/following-sibling::div/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "turnover_rate": {
                "paths": [
                    "//div[contains(text(),'Annual Turnover')]/following-sibling::div/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "turnover_rate_date": {
                "paths": [
                    "(//div[contains(text(),'As of')]/text())[1]"
                ],
                "cleanup_functions": [
                    "obj['turnover_rate_date'].split('As of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h3[contains(text(),'Management')]/parent::div/div/a/div[2]"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}