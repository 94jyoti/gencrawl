{
    "website": "https://www.cimgroup.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "cimgroup.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.cimgroup.com/public-investment-programs/current-public-programs/racr"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "(//h3[@class = 'field-title']/following-sibling::div/p)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//p[contains(text(),'Class') and not(contains(text(),'CIM'))]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./parent::td/following-sibling::td[1]/p/text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_consultant": {
                "paths": [
                    "//p[contains(text(),'Advisor') and not(contains(text(),'Sub-Advisor'))]/strong/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//p[contains(text(),'Inception ')]/text()"
                ],
                "cleanup_functions": [
                    "obj['share_inception_date'].split(':')[-1]",
                    "obj['share_inception_date'].split(')')[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sub_advisor": {
                "paths": [
                    "//p[contains(text(),'Sub-Advisor')]/strong/span/text()"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}