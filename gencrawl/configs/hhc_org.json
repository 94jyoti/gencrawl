{
    "website": "https://www.hhc.org/",
    "domain": "hospital",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "hhc.org"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.hhc.org/find-a-physician/maher-a-abdulla-md/",
            "https://www.hhc.org/find-a-physician/omar-abumahfouz-md/",
            "https://www.hhc.org/find-a-physician/deep-adhikari-md/",
            "https://www.hhc.org/find-a-physician/chinenye-adimora-md/",
            "https://www.hhc.org/find-a-physician/rafael-j-aguila-md/",
            "https://www.hhc.org/find-a-physician/mueez-ahmed-md-facc/",
            "https://www.hhc.org/find-a-physician/iyad-al-husein-md/",
            "https://www.hhc.org/find-a-physician/yasir-al-qaqaa-md/",
            "https://www.hhc.org/find-a-physician/amer-al-rafati-md-faap/",
            "https://www.hhc.org/find-a-physician/mohammad-n-al-shroof-md/",
            "https://www.hhc.org/find-a-physician/mousa-al-wawi-md/",
            "https://www.hhc.org/find-a-physician/victor-j-andress-md/"
        ],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "//article[@id='PhysicianContent']/h1//text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "speciality": {
                "paths": [
                    "//*[contains(text(), 'Primary Specialty')]/parent::li/span/span/a/text()|//*[contains(text(), 'Primary Specialties')]/parent::li/span/span/a/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "affiliation": {
                "paths": [
                    "//*[contains(text(), 'Affiliations')]/parent::li/span/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "address_raw": {
                "paths": [
                    "//*[contains(text(), 'Location Information')]/parent::header/following-sibling::ul//address"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "practice_name": {
                "paths": [
                    "./preceding::strong[1]/text()"
                ],
                "return_type": "str",
                "selector": "address_raw"
            }
        }
    }
}