{
    "website": "https://www.fullerthalerfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "fullerthalerfunds.com",
        "wix-visual-data.appspot.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "iframe": "//iframe",
        "start_urls": [
            "https://www.fullerthalerfunds.com/about",
            "https://www.fullerthalerfunds.com/ftxsx",
            "https://www.fullerthalerfunds.com/ftvsx",
            "https://www.fullerthalerfunds.com/unconstrained-equity-ftzix",
            "https://www.fullerthalerfunds.com/micro-cap-ftmsx",
            "https://www.fullerthalerfunds.com/small-mid-core-ftsix"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h4/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//td[contains(text(),'Ticker Symbol')]/following-sibling::td"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Gross Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Net Expense Ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Initial Investment')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            }
        }
    }
}