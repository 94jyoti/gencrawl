{
    "website": "https://entrepreneurshares.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "entrepreneurshares.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://entrepreneurshares.com/ershares-mutual-funds/implx/",
            "https://entrepreneurshares.com/ershares-mutual-funds/entix/",
            "https://entrepreneurshares.com/ershares-mutual-funds/impax/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[contains(text(),'Fund') or contains(text(),'fund')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//div[contains(text(),'Fund') or contains(text(),'fund')]/parent::h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//span[contains(text(),'CUSIP')]/following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "asset_class": {
                "paths": [
                    "//span[contains(text(),'Fund Type')]/following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//span[contains(text(),'AUM')]/following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h3[contains(text(),'Fund Details')]/following-sibling::div/small/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "redemption_fee": {
                "paths": [
                    "//i[contains(text(),'Redemption')]/text()"
                ],
                "cleanup_functions": [
                    "obj['redemption_fee'].split('Redemption Fee of')[-1].split('%')[0] +\"%\""
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//span[contains(text(),'Gross Expense Ratio')]/following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//span[contains(text(),'Net Expense Ratio')]/following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//span[contains(text(),'Inception Date')]/following-sibling::span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h3[contains(text(),'Fund Strategy')]/following-sibling::p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            }
        }
    }
}