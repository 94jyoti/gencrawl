{
    "website": "https://www.pappmutualfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "pappmutualfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_pappmutualfunds_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.pappmutualfunds.com/pappx.html"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//title/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//strong[contains(text(),'Ticker')]/parent::td/parent::tr/following-sibling::tr[1]/td//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_consultant": {
                "paths": [
                    "//strong[contains(text(),'Adviser')]/parent::span/following-sibling::span/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "total_net_assets": {
                "paths": [
                    "//span[contains(text(),'Total Assets')]/parent::td/following-sibling::td/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//td[@class='body-blue']/strong[1][contains(text(),'As of')]/text()"
                ],
                "cleanup_functions": [
                    "obj['total_net_assets_date'].split('As of')[-1] "
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//strong[contains(text(),'Sales Charge')]/parent::td/parent::tr/following-sibling::tr[1]/td[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//span[contains(text(),'Gross')]/parent::td/following-sibling::td/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//span[contains(text(),'Net')]/parent::td/following-sibling::td/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//span[contains(text(),'Inception')]/parent::td/following-sibling::td/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//p[contains(text(),'Objective')]/span/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//span[contains(text(),'Philosophy & Strategy')]/following-sibling::span/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//span[contains(text(),'Initial')]/parent::td/following-sibling::td/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//span[contains(text(),'Additional')]/parent::td/following-sibling::td/span/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}