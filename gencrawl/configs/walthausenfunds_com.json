{
    "website": "http://www.walthausenfunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "walthausenfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "http://www.walthausenfunds.com/the-funds/walthausen-small-cap-value-fund/",
            "http://www.walthausenfunds.com/the-funds/walthausen-focused-select-value-fund/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h2[contains(text(),'Fund') and not(contains(text(),'Documents'))]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//strong[contains(text(),'CUSIP')]/parent::div"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//strong[contains(text(),'TICKER SYMBOL')]/parent::div/text()"
                ],
                "return_type": "list-map",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "//strong[contains(text(),'Fund') and(contains(text(),'Class'))]/text()"
                ],
                "cleanup_functions": [
                    "[s.split('Fund')[-1] for s in obj['share_class']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//*[contains(text(),'Objective')]/following::p[contains(text(),'seeks')]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//*[contains(text(),'Investment Strategies')]/following::p[position()<4]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Regular')]/following-sibling::td[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//td[contains(text(),'Regular')]/following-sibling::td[2]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            }
        }
    }
}