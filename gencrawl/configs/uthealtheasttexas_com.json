{
    "website": "https://uthealtheasttexas.com/",
    "domain": "hospital",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "uthealtheasttexas.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "hospital_detail",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [],
        "ext_codes": {
            "raw_full_name": {
                "paths": [
                    "(//h1)[1]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "speciality": {
                "paths": [
                    "((//div[contains(text(),'Specialty')])[1]//following::div[1]/div)[1]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "address_raw": {
                "paths": [
                    "//div[contains(@class,\"address field-type\")]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "phone": {
                "paths": [
                    "//div[contains(@class,\"phone field-type\")]//div/text()"
                ],
                "return_type": "list",
                "selector": "root"
            }
        }
    }
}