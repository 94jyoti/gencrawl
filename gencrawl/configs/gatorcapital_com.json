{
    "website": "https://gatorcapital.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "gatorcapital.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_gator_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://gatorcapital.com/mutual-funds/gator-capital-long-short-fund/gator-capital-long-short-fund-returns/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]//following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]//following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "(//div[@class=\"right\"]/table//following::td[1])[4]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "(//div[@class=\"right\"]//p)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//td[contains(text(),'Management Fee')]//following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Gross Expense Ratio')]//following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "(//div[@class=\"right\"]/table//following::td[1])[2]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Initial Investment')]//following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//th[contains(text(),'Benchmark')]/following::tbody[1]/tr/td[1][not(em)]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}