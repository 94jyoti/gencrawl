{
    "website": "https://www.delawarefunds.com",
    "spider": "financial_detail",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "www.delawarefunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
         "custom_settings": {
             "HTTPCACHE_ENABLED": false,
             "CRAWLERA_PRESERVE_DELAY": true
        },
        "start_urls": [
            "https://www.delawarefunds.com/products/"
        ],
         "pagination":{
            "temp_url": {
                "paths": [
                    "//form[@id='mainform']/@action"
                ],
                "cleanup_functions": [
                    "[obj['temp_url'].split('FundClass=')[0] + 'FundClass=']"
                ]
            },
            "pagination": {
                "paths": [
                    "//option[position()>1]//@value"
                ],
                "return_type": "list",
                "cleanup_functions": [
                    "[p + i for p, i in zip(obj['temp_url']*len(obj['pagination']), obj['pagination'])]"

                ]
            }
        },
        "ext_codes": {
            "retry_condition": {
                "paths": [
                    "//*[contains(text(), 'NaN million')]"
                ]
            },
            "share_class": {
                "paths": [
                    "//option[@selected=\"selected\"]//text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split(\"(\")[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//table[@id=\"fundIdentifier\"]//td[contains(text(),'NASDAQ')]//following-sibling::td//text()",
                    "//option[@selected='selected']//text()"
                ],
                "return_type": "str",
                "selector": "root",
                "cleanup_functions": ["obj['nasdaq_ticker'].split(\"(\")[-1].replace(\")\",\"\")"]
            },
            "cusip": {
                "paths": [
                    "//table[@id=\"fundIdentifier\"]//td[contains(text(),'CUSIP')]//following-sibling::td//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "instrument_name": {
                "paths": [
                    "//*[@id=\"bdBody\"]//h1//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity": {
                "paths": [
                    "//div[contains(text(),'Effective maturity')]//following-sibling::div//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration": {
                "paths": [
                    "//div[contains(text(),'Effective duration (weighted average)')]//following-sibling::div//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration_date": {
                "paths": [
                    "//h4[contains(text(),' Portfolio characteristics ')]//small//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "average_weighted_maturity_as_of_date": {
                "paths": [
                    "//h4[contains(text(),' Portfolio characteristics ')]//small//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//section[@id='managers']"
                ],
                "return_type": "selector",
                "child_return_strategy": "multiple_objects"
            },
            "fund_manager": {
                "paths": [
                    ".//h4//text()"
                ],
                "cleanup_functions":["obj['fund_manager'].replace('&nbsp','').strip()"],
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_in_industry": {
                "paths": [
                    ".//li[@class=\"years-of-experience\"]//text()"
                ],
                "cleanup_functions": [
                    "obj['fund_manager_years_of_experience_in_industry'].split(\":\")[-1].strip()"
                ],
                "selector": "temp_fund_managers"
            },
            "fund_manager_years_of_experience_with_fund": {
                "paths": [
                    ".//li[@class=\"fund-start-date\"]//text()"
                ],
                "cleanup_functions": [
                    "obj['fund_manager_years_of_experience_with_fund'].split(\":\")[-1].strip()"
                ],
                "selector": "temp_fund_managers"
            },
            "temp_bench":{
             "paths":["//div[@class='benchmark-peer-group-wrap']//p//strong"],
            "return_type": "selector",
            "child_return_strategy": "single_item"
            
            },
            "benchmarks": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "join",
                "selector": "temp_bench"
            },
            "total_expense_gross": {
                "paths": [
                    "//div[text()=('Gross')]//following-sibling::div//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//div[text()=('Net')]//following-sibling::div//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception date')]//following-sibling::td//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "maximum_sales_charge_full_load": {
                "paths": [
                    "//td[@id='maxSalesCharge']//following-sibling::td//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets": {
                "paths": [
                    "//h4[contains(text(),'Total net assets')]//parent::div//div[2]//text()"
                ],
                "return_type": "str",
                "selector": "root",
                "cleanup_functions": [
                    "obj['portfolio_assets'].strip()"]
            },
            "portfolio_assets_date": {
                "paths": [
                    "//h4[contains(text(),'Total net assets')]//small//text()"
                ],
                "return_type": "str",
                "selector": "root",
            	"cleanup_functions": [
                    "obj['portfolio_assets_date'].replace('\\r\\t\\n', '').strip()"]
            },
            "sec_yield_30_day": {
                "paths": [
                    "//div[contains(text(),'SEC 30-day yield with waiver ')]//following-sibling::div//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "//h4[contains(text(),' Portfolio characteristics ')]//small//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_30_day": {
                "paths": [
                    "//div[contains(text(),'SEC 30-day yield without waiver')]//following-sibling::div//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_date_30_day": {
                "paths": [
                    "//h4[contains(text(),' Portfolio characteristics ')]//small//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "annual_fund_operating_expenses_after_fee_waiver": {
                "paths": [
                    "//td[@id='totalAnnualExpensesAfterWaivers']//following-sibling::td//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//td[@id='distributionServiceFees']//following-sibling::td//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "other_expenses": {
                "paths": [
                    "//td[@id='otherExpenses']//following-sibling::td//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "management_fee": {
                "paths": [
                    "//td[@id='managementFees']//following-sibling::td//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "expense_waivers": {
                "paths": [
                    "//td[@id='feeWaiversExpenseReimbursements']//following-sibling::td//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "dividend_frequency":{
             "paths":["//table[@id='fundInformation']//td[contains(text(),'Dividends')]//following-sibling::td//text()"]
            }
            ,
            "temp_capital_gains":{
              "paths":["//*[@id='distributionHistory']/div"],
              "child_return_strategy": "multiple_objects",
              "return_type": "selector",
              "selector": "root"
            },
            "cg_ex_date":{
             "paths":[".//div[1]//text()"],
             "return_type": "str",
             "selector": "temp_capital_gains"
            },
            "ordinary_income":{
             "paths":[".//div[3]//text()"],
             "return_type": "str",
             "selector": "temp_capital_gains"
            },
            "total_per_share":{
             "paths":[".//div[2]//text()"],
             "return_type": "str",
             "selector": "temp_capital_gains"
            },
            "turnover_rate":{
             "paths":["//div[contains(text(),'turnover')]//following-sibling::div//text()"]
            },
            "turnover_rate_date":{
             "paths":["//h4[contains(text(),' Portfolio characteristics ')]//small//text()"]
            },
            "contingent_deferred_sales_charge":{
             "paths":["//td[@id='maxContingentDeferredSalesCharge']//following-sibling::td//text()"]
            }
        }
    }
}