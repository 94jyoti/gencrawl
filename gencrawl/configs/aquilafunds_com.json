{
    "website": "https://aquilafunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "aquilafunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_aquila_com",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://aquilafunds.com/funds/arizona-funds/",
            "https://aquilafunds.com/funds/aquila-tax-free-fund-of-colorado/",
            "https://aquilafunds.com/funds/hawaiian-tax-free-trust/",
            "https://aquilafunds.com/funds/aquila-churchill-tax-free-fund-of-kentucky/",
            "https://aquilafunds.com/funds/aquila-tax-free-trust-of-oregon/",
            "https://aquilafunds.com/funds/aquila-narragansett-ri-tax-free-income-fund/",
            "https://aquilafunds.com/funds/aquila-tax-free-fund-for-utah/",
            "https://aquilafunds.com/funds/aquila-three-peaks-income-fund/",
            "https://aquilafunds.com/funds/aquila-three-peaks-opportunity-growth-fund-2/"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//header[@class='entry-header']/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//div[@id='home-slide-1']/section/label"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//li[contains(text(),'Total net assets')]/span/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//li[contains(text(),'Total net assets')]/text()"
                ],
                "cleanup_functions": [
                    "[o.split(':')[0] for o in obj['portfolio_assets_date']]",
                    "[o.split('as of')[-1] for o in obj['portfolio_assets_date']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "fees_total_12b_1": {
                "paths": [
                    "//th[contains(text(),'12b-1 Fees')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//th[contains(text(),'Total Operating Expense')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//th[contains(text(),'Net Expense ratio')]/following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//li[contains(text(),'Inception date')]/span/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h3[contains(text(),'Description & Objective')]/following-sibling::p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            }
        }
    }
}