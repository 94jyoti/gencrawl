{
    "website": "https://www.alpsfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "alpsfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.alpsfunds.com/products/mutual-fund/JCRAX",
            "https://www.alpsfunds.com/products/mutual-fund/INDAX",
            "https://www.alpsfunds.com/products/mutual-fund/LPEFX",
            "https://www.alpsfunds.com/products/mutual-fund/ALIBX",
            "https://www.alpsfunds.com/products/mutual-fund/SMCVX",
            "https://www.alpsfunds.com/products/mutual-fund/SMRSX",
            "https://www.alpsfunds.com/products/mutual-fund/SMTRX",
            "https://www.alpsfunds.com/products/mutual-fund/CHNAX",
            "https://www.alpsfunds.com/products/mutual-fund/RLTAX",
            "https://www.alpsfunds.com/products/mutual-fund/RLGAX",
            "https://www.alpsfunds.com/products/mutual-fund/RMIAX"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "(//h1)[1]/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "(.//*[contains(text(),'Symbol')])//following-sibling::*[1]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "cusip": {
                "paths": [
                    "(.//*[contains(text(),'CUSIP')])//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "isin": {
                "paths": [
                    ".//*[contains(text(),'ISIN')]//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "asset_class": {
                "paths": [
                    "(.//*[contains(text(),'Fund Type')])//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_net_assets": {
                "paths": [
                    "(.//*[contains(text(),'Total Net Assets')])//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "(.//*[contains(text(),'Total Expense Ratio')])//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    ".//*[contains(text(),'Inception Date')]//following-sibling::*[1]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    ".//*[contains(text(),'STRATEGY')]//following::p[@class='MsoPlainText'][1]//text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "distribution_frequency": {
                "paths": [
                    "(.//*[contains(text(),'Distribution Frequency')])//following-sibling::*[1]//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    ".//*[contains(text(),'Committee Members')]//following::p[@class='MsoPlainText']//strong"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}