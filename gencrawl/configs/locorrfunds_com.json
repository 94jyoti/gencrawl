{
    "website": "https://www.locorrfunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "locorrfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SCRAPY_GET",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.locorrfunds.com/funds/managed_futures_strategy.html",
            "https://www.locorrfunds.com/funds/long_short_commodities_strategy.html",
            "https://www.locorrfunds.com/funds/spectrum_income.html",
            "https://www.locorrfunds.com/funds/long_short_equity.html",
            "https://www.locorrfunds.com/funds/market_trend.html"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//div[@class = 'page-header']/div/h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//th[contains(text(),'Class')]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//th[contains(text(),'Ticker')]/ancestor::thead/following-sibling::tbody/tr/td[1]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//th[contains(text(),'CUSIP')]/ancestor::thead/following-sibling::tbody/tr/td[2]/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "portfolio_assets": {
                "paths": [
                    "//td[contains(text(),'Total Fund Net Assets')]//following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "portfolio_assets_date": {
                "paths": [
                    "//div[contains(text(),'Period Ending')]/text()"
                ],
                "cleanup_functions": [
                    "obj['portfolio_assets_date'].split('Ending ')[-1]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Gross Expense Ratio')]//following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Net Expense Ratio')]//following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]//following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//td[contains(text(),'Minimum Investment')]//following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "minimum_additional_investment": {
                "paths": [
                    "//td[contains(text(),'Subsequent Investment')]//following-sibling::td/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            }
        }
    }
}