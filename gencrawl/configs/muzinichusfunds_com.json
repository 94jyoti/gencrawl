{
    "website": "https://www.muzinichusfunds.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "muzinichusfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.muzinichusfunds.com/strategies/credit_opp_performance",
            "https://www.muzinichusfunds.com/strategies/high_income_performance",
            "https://www.muzinichusfunds.com/strategies/high_yield_performance",
            "https://www.muzinichusfunds.com/strategies/low_duration_performance"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//table[@class=\"fund_table\"]//tbody//tr[2]//td[1]//text()"
                ],
                "cleanup_functions": [
                    "obj['instrument_name'].split(\"\u2013\")[0]",
                    "obj['instrument_name'].split(\"-\")[0]"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//table[@class='fund_table']/tbody/tr/td[1]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "cleanup_functions": [
                    "obj['share_class'].split(\"\u2013\")[-1]"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "total_expense_gross": {
                "paths": [
                    "//p[contains(text(),'Expense Ratio')]/text()",
                    "//p[contains(text(),'Since Inception ')]/text()[contains(.,'Gross Expense')]"
                ],
                "cleanup_functions": [
                    "[re.findall(r'\\d*\\.?\\d+',i)[0] for i in obj['total_expense_gross']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//p[contains(text(),'Expense Ratio')]/text()",
                    "//p[contains(text(),'Since Inception ')]/text()[contains(.,'Gross Expense')]"
                ],
                "cleanup_functions": [
                    "[re.findall(r'\\d*\\.?\\d+',i)[-1] for i in obj['total_expense_net']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "effective_duration": {
                "paths": [
                    "//td[contains(text(),'Average Duration (yrs)')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "effective_duration_date": {
                "paths": [
                    "//th[contains(text(),'Portfolio Characteristics')]//text()"
                ],
                "cleanup_functions": [
                    "obj['effective_duration_date'].replace('Portfolio Characteristics as of ', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_30_day": {
                "paths": [
                    "//td[contains(text(),'30-Day SEC Yield with waiver')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_date_30_day": {
                "paths": [
                    "//th[contains(text(),'Portfolio Characteristics')]//text()"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_date_30_day'].replace('Portfolio Characteristics as of ', '')"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_30_day": {
                "paths": [
                    "//td[contains(text(),'30-Day SEC Yield w/o waiver')]/following-sibling::td/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "sec_yield_without_waivers_date_30_day": {
                "paths": [
                    "//th[contains(text(),'Portfolio Characteristics')]//text()"
                ],
                "cleanup_functions": [
                    "obj['sec_yield_without_waivers_date_30_day'].replace('Portfolio Characteristics as of ', '')"
                ],
                "return_type": "str",
                "selector": "root"
            }
        }
    }
}