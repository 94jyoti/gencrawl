{
    "website": "https://www.cloughglobal.com",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "cloughglobal.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.cloughglobal.com/long-short"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//span[@class='hero-short__title']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//a[contains(text(),'Class')]"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//td[contains(text(),'Ticker')]/span/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//td[contains(text(),'CUSIP')]/span/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_class": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "total_net_assets": {
                "paths": [
                    "//td[contains(text(),'Total Net Assets')]/span/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_net_assets_date": {
                "paths": [
                    "//span[@id='daily_asofdate_i']/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "annual_fund_operating_expenses_after_fee_waiver": {
                "paths": [
                    "//td[contains(text(),'Operating Expenses')]/span/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//td[contains(text(),'Total Expense Ratio')]/span/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//td[contains(text(),'Total Net Expense Ratio')]/span/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "share_inception_date": {
                "paths": [
                    "//td[contains(text(),'Inception Date')]/span/text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h3[contains(text(),'Objective')]/parent::div/p[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//h3[contains(text(),'Performance')]/parent::div//h3/following-sibling::table//tr/td[contains(text(),'Index')]"
                ],
                "cleanup_functions": [
                    "list(set(obj['benchmarks']))"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//h3[contains(text(),'Fund Management')]/parent::div/parent::div/following-sibling::div/div/div/p/b"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}