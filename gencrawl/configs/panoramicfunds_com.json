{
    "website": "https://www.panoramicfunds.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "panoramicfunds.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail",
        "crawl_method": "SELENIUM",
        "wait_time": 3,
        "custom_settings": {
            "HTTPCACHE_ENABLED": false
        },
        "parsing_type": "xpath",
        "start_urls": [
            "https://www.panoramicfunds.com/Funds/Mutual-Funds/US-Equity-Fund",
            "https://www.panoramicfunds.com/Funds/Mutual-Funds/International-Equity-Fund",
            "https://www.panoramicfunds.com/Funds/Mutual-Funds/Global-Equity-Fund",
            "https://www.panoramicfunds.com/Funds/Mutual-Funds/Tax-Managed-Global-Equity-Fund",
            "https://www.panoramicfunds.com/Funds/Mutual-Funds/US-Fixed-Income-Fund",
            "https://www.panoramicfunds.com/Funds/Mutual-Funds/Municipal-Fixed-Income-Fund",
            "https://www.panoramicfunds.com/Funds/Mutual-Funds/Global-Fixed-Income-Fund",
            "https://www.panoramicfunds.com/Funds/Mutual-Funds/Alternatives-Fund"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//h1/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "//dt[contains(text(),'Fund Ticker')]/following-sibling::dd/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "cusip": {
                "paths": [
                    "//dt[contains(text(),'CUSIP')]/following-sibling::dd/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_gross": {
                "paths": [
                    "//dt[contains(text(),'Gross Expense Ratio')]/following-sibling::dd/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "total_expense_net": {
                "paths": [
                    "//dt[contains(text(),'Net Expense Ratio')]/following-sibling::dd/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h2[contains(text(),'Investment Objective')]/following-sibling::p[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h2[contains(text(),'Strategy')]/following-sibling::p[1]/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "minimum_initial_investment": {
                "paths": [
                    "//dt[contains(text(),'Initial Minimum Investment')]/following-sibling::dd/text()"
                ],
                "return_type": "str",
                "selector": "root"
            },
            "benchmarks": {
                "paths": [
                    "//h3[contains(text(),'Benchmark')]/following-sibling::p[1]/text()"
                ],
                "return_type": "list",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class='slick-slide' or contains(@class, 'slick-active')]//h3[@class='portfolio-manager__name']//a"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    "./text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}