{
    "website": "https://www.brookfield.com/",
    "domain": "financial",
    "crawl_method": "SCRAPY_GET",
    "allowed_domains": [
        "brookfield.com"
    ],
    "parsing_type": "xpath",
    "country": "US",
    "language": "ENGLISH",
    "detail": {
        "spider": "financial_detail_field_mapping",
        "crawl_method": "SELENIUM",
        "parsing_type": "xpath",
        "start_urls": [
            "https://publicsecurities.brookfield.com/products/us-mutual-funds/brookfield-global-listed-real-estate-fund?id=190651",
            "https://publicsecurities.brookfield.com/products/us-mutual-funds/brookfield-global-listed-infrastructure-fund?id=190652",
            "https://publicsecurities.brookfield.com/products/us-mutual-funds/center-coast-brookfield-midstream-focus-fund?id=192681",
            "https://publicsecurities.brookfield.com/products/us-mutual-funds/brookfield-real-assets-securities-fund?id=192674"
        ],
        "ext_codes": {
            "instrument_name": {
                "paths": [
                    "//span[contains(text(),'Public Securities')]/parent::div/h1//text()"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "temp_class_blocks": {
                "paths": [
                    "//div[@class='rightSectionDetails']//select/option"
                ],
                "child_return_strategy": "multiple_items",
                "return_type": "selector",
                "selector": "root"
            },
            "nasdaq_ticker": {
                "paths": [
                    "./@value"
                ],
                "return_type": "str",
                "selector": "temp_class_blocks"
            },
            "share_class": {
                "paths": [
                    "//select[@selected]/option/text()"
                ],
                "cleanup_functions": [
                    "[o.split('(')[0] for o in obj['share_class']]"
                ],
                "return_type": "list-map",
                "selector": "root"
            },
            "investment_objective": {
                "paths": [
                    "//h3[contains(text(),'Investment objective and strategy')]/parent::div/p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "investment_strategy": {
                "paths": [
                    "//h3[contains(text(),'Investment objective and strategy')]/parent::div/p/text()"
                ],
                "return_type": "join",
                "selector": "root"
            },
            "temp_fund_managers": {
                "paths": [
                    "//div[@class=\"managers-container\"]//a"
                ],
                "child_return_strategy": "multiple_objects",
                "return_type": "selector",
                "selector": "root"
            },
            "fund_manager": {
                "paths": [
                    ".//div//span[1]//text()"
                ],
                "return_type": "str",
                "selector": "temp_fund_managers"
            }
        }
    }
}